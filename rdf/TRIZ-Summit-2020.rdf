    <od:presentedBy rdf:resource="http://opendiscovery.org/rdf/Person/Saunin_N"/>
    <od:urlPaper rdf:resource="https://r1.nubex.ru/s828-c8b/f3243_b6/%D0%A4%D0%BE%D0%BC%D0%B5%D0%BD%D0%BA%D0%BE-%D0%A1%D0%B0%D1%83%D0%BD%D0%B8%D0%BD-TDS2010_Business[1].pdf"/>
