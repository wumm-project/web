  <od:Talk rdf:about="http://opendiscovery.org/rdf/Conferences/TRIZ-Summit-2020/Talk_101">
    <swc:relatedToEvent rdf:resource="http://opendiscovery.org/rdf/Conferences/TRIZ-Summit-2020/track/Edu"/>
    <dcterms:creator rdf:resource="http://opendiscovery.org/rdf/Person/Rubin_MS"/>
    <dcterms:creator rdf:resource="http://opendiscovery.org/rdf/Person/Rubina_NV"/>
