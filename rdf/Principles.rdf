    <skos:altLabel xml:lang="en">Nested dolls</skos:altLabel>
    <skos:prefLabel xml:lang="en">Principle of nesting (matryoshka)</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Prinzip der Integration (Matrjoschka-Prinzip)</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Приём вложенности («матрёшки»)</skos:prefLabel>
  </od:Principle>
