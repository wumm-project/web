    <skos:prefLabel xml:lang="de">Gesonderte Behandlung</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Principle of separation</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Prinzip der Abtrennung</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Приём вынесения</skos:prefLabel>
  </od:Principle>
