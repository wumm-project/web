<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF xmlns:cc="http://creativecommons.org/ns#"
   xmlns:dcterms="http://purl.org/dc/terms/"
   xmlns:od="http://opendiscovery.org/rdf/Model#"
   xmlns:odp="http://opendiscovery.org/rdf/Principle/"
   xmlns:owl="http://www.w3.org/2002/07/owl#"
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
   xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
   xmlns:skos="http://www.w3.org/2004/02/skos/core#"
   xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
  <owl:Class rdf:about="http://opendiscovery.org/rdf/Model#Principle">
    <rdfs:label>A TRIZ Principle</rdfs:label>
  </owl:Class>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P01">
    <od:description xml:lang="de">Das Objekt ist in unabhängige, gleiche Teile zu zerlegen.</od:description>
    <od:description xml:lang="de">Das Objekt ist zerlegbar auszuführen.</od:description>
    <od:description xml:lang="de">Der Grad der Zerlegung des Objektes ist zu erhöhen.</od:description>
    <od:hasPrincipleNumber>01</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip der Zerlegung bzw. Segmentierung</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P02">
    <od:description xml:lang="de">Im Unterschied zum Prinzip 01, in dem es um die Zerlegung des Objektes in gleiche Teile ging, wird hier vorgeschlagen, das Objekt in unterschiedliche Teile zu zerlegen.</od:description>
    <od:description xml:lang="de">Vom Objekt ist das "störende" Teil, die "störende" Eigenschaft, abzutrennen.</od:description>
    <od:hasPrincipleNumber>02</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip der Abtrennung</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P03">
    <od:description xml:lang="de">Jedes Teil des Objektes soll sich unter solchen Bedingungen befinden, die seiner Arbeit am zuträglichsten sind.</od:description>
    <od:description xml:lang="de">Von der homogenen Struktur des Objektes oder des umgebenden Mediums ist zu einer inhomogenen Struktur überzugehen.</od:description>
    <od:hasPrincipleNumber>03</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip der örtlichen Qualität</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P04">
    <od:description xml:lang="de">Der Grad der Asymmetrie ist zu erhöhen, wenn das Objekt bereits asymmetrisch ist.</od:description>
    <od:description xml:lang="de">Von der symmetrischen Form des Objektes ist zur asymmetrischen überzugehen.</od:description>
    <od:hasPrincipleNumber>04</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip der Asymmetrie</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P05">
    <od:description xml:lang="de">Gleichartige oder zur Koordinierung bestimmte Systeme oder Operationen sind zu koppeln.</od:description>
    <od:hasPrincipleNumber>05</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip der Kopplung</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P06">
    <od:description xml:lang="de">Das Objekt erfüllt mehrere unterschiedliche Funktionen, wodurch weitere gesonderte Objekte überflüssig werden.</od:description>
    <od:hasPrincipleNumber>06</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip der Universalität</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P07">
    <od:description xml:lang="de">Ein Objekt durchläuft oder füllt den Hohlraum eines anderen Objektes.</od:description>
    <od:description xml:lang="de">Ein Objekt ist im Inneren eines anderen untergebracht, das sich wiederum im Inneren eines dritten befindet.</od:description>
    <od:hasPrincipleNumber>07</od:hasPrincipleNumber>
    <rdfs:comment xml:lang="de">Auch als Prinzip der "Steckpuppe" (Matrjoschka) bezeichnet</rdfs:comment>
    <rdfs:label xml:lang="de">Prinzip der Verschachtelung</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P08">
    <od:description xml:lang="de">Die Masse des Objektes ist durch Kopplung mit einem anderen Objekt, das Tragkraft besitzt, zu kompensieren.</od:description>
    <od:description xml:lang="de">Die Masse des Objektes ist durch Wechselwirkung mit einem Medium zu kompensieren.</od:description>
    <od:hasPrincipleNumber>08</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip der Gegenmasse</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P09">
    <od:description xml:lang="de">Wenn gemäß den Bedingungen der Aufgabe eine bestimmte Wirkung erzielt werden soll, muss zuvor die Gegenwirkung erzeugt werden.</od:description>
    <od:hasPrincipleNumber>09</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip der vorgezogenen Gegenwirkung</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P10">
    <od:description xml:lang="de">Die Objekte sind vorher so aufzustellen bzw. einzusetzen, dass sie ohne Zeitverlust für ihr Herbeischaffen vom geeignetsten Ort aus wirken können.</od:description>
    <od:description xml:lang="de">Die erforderliche Wirkung ist vorher zu erzielen (vollständig oder auch teilweise).</od:description>
    <od:hasPrincipleNumber>10</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip der vorgezogenen Wirkung</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P11">
    <od:description xml:lang="de">Eine relativ geringe Zuverlässigkeit des Objektes wird durch vorher bereitgestellte Hilfsmittel ausgeglichen.</od:description>
    <od:hasPrincipleNumber>11</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip des "vorher untergelegten Kissens" (Prävention)</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P12">
    <od:description xml:lang="de">Die Arbeitsbedingungen sind so zu verändern, dass das Objekt mit konstantem Energiepotenzial arbeiten kann, z.B. nicht angehoben oder herabgelassen werden muss.</od:description>
    <od:hasPrincipleNumber>12</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip des Äquipotenzials</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P13">
    <od:description xml:lang="de">Das Objekt ist "auf den Kopf zu stellen" bzw. umzukehren.</od:description>
    <od:description xml:lang="de">Der bewegliche Teil des Objektes oder des umgebenden Mediums ist unbeweglich, und der unbewegliche ist beweglich zu gestalten.</od:description>
    <od:description xml:lang="de">Statt der Wirkung, die durch die Bedingungen der Aufgabe vorgeschrieben wird, ist die umgekehrte Wirkung zu erzielen.</od:description>
    <od:hasPrincipleNumber>13</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip der Funktionsumkehr</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P14">
    <od:description xml:lang="de">Von der geradlinigen Bewegung ist zur Rotation überzugehen.</od:description>
    <od:description xml:lang="de">Von geradlinigen Konturen ist zu gekrümmten, von ebenen Flächen ist zu sphärischen überzugehen.</od:description>
    <od:description xml:lang="de">Zu verwenden sind Rollen, Kugeln, Spiralen.</od:description>
    <od:hasPrincipleNumber>14</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip der Kugelähnlichkeit</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P15">
    <od:description xml:lang="de">Das Objekt ist in Teile zu zerlegen, die sich zueinander verstellen oder verschieben lassen.</od:description>
    <od:description xml:lang="de">Die Kennwerte des Objektes (oder des umgebenden Mediums) müssen sich so verändern, dass sie in jeder Arbeitsetappe optimal sind (Anpassung).</od:description>
    <od:description xml:lang="de">Falls das Objekt insgesamt unbeweglich ist, ist es beweglich (verstellbar) zu gestalten.</od:description>
    <od:hasPrincipleNumber>15</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip der Dynamisierung</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P16">
    <od:description xml:lang="de">Wenn 100 % des erforderlichen Effekts schwer zu erzielen sind, muss „ein bisschen weniger“ oder „ein bisschen mehr“  erzielt werden.</od:description>
    <od:hasPrincipleNumber>16</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip der partiellen oder überschüssigen Wirkung</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P17">
    <od:description xml:lang="de">Das Objekt ist geneigt aufzustellen.</od:description>
    <od:description xml:lang="de">Die Rückseite des gegebenen Objektes ist auszunutzen.</od:description>
    <od:description xml:lang="de">Schwierigkeiten, die aus der Bindung der Bewegung eines Objektes an eine Linie resultieren, werden beseitigt, wenn das Objekt die Möglichkeit erhält, sich in zwei Dimensionen, d. h. in einer Ebene, zu bewegen. Analog werden auch die Schwierigkeiten, die mit der Bewegung von Objekten auf einer Ebene verbunden sind, beim Übergang zum dreidimensionalen Raum beseitigt.</od:description>
    <od:description xml:lang="de">Statt Anordnung in nur einer Ebene werden Objekte in mehreren Ebenen angeordnet.</od:description>
    <od:hasPrincipleNumber>17</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip des Übergangs zu höheren Dimensionen</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P18">
    <od:description xml:lang="de">Anstelle von mechanischen Vibratoren sind Piezovibratoren anzuwenden.</od:description>
    <od:description xml:lang="de">Auszunutzen sind Ultraschallschwingungen in Verbindung mit elektromagnetischen Feldern.</od:description>
    <od:description xml:lang="de">Das Objekt ist in Schwingungen zu versetzen.</od:description>
    <od:description xml:lang="de">Die Eigenfrequenz ist auszunutzen.</od:description>
    <od:description xml:lang="de">Falls eine solche Bewegung bereits vorliegt, ist ihre Frequenz zu erhöhen, bis hin zur Ultaschallfrequenz.</od:description>
    <od:hasPrincipleNumber>18</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip der Ausnutzung mechanischer Schwingungen</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P19">
    <od:description xml:lang="de">Die Pausen zwischen den Impulsen sind für eine andere Wirkung auszunutzen.</od:description>
    <od:description xml:lang="de">Von der kontinuierlichen Wirkung ist zur periodischen (Impulswirkung) überzugehen.</od:description>
    <od:description xml:lang="de">Wenn die Wirkung bereits periodisch erfolgt, ist die Periodizität zu verändern.</od:description>
    <od:hasPrincipleNumber>19</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip der periodischen Wirkung</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P20">
    <od:description xml:lang="de">Die Funktion soll kontinuierlich erfolgen (alle Teile des Objektes sollen ständig mit gleichmäßiger Belastung arbeiten).</od:description>
    <od:description xml:lang="de">Leerläufe und Unterbrechungen sind auszuschalten.</od:description>
    <od:hasPrincipleNumber>20</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip der Kontinuität der Wirkprozesse</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P21">
    <od:description xml:lang="de">Der Prozess oder einzelne seiner Etappen, z. B. schädliche oder gefährliche, sind mit hoher Geschwindigkeit zu durchlaufen.</od:description>
    <od:hasPrincipleNumber>21</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip des Durcheilens</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P22">
    <od:description xml:lang="de">Ein schädlicher Faktor ist bis zu einem solchen Grade zu verstärken, dass er aufhört, schädlich zu sein.</od:description>
    <od:description xml:lang="de">Ein schädlicher Faktor ist durch Überlagerung mit anderen schädlichen Faktoren zu beseitigen.</od:description>
    <od:description xml:lang="de">Schädliche Faktoren – insbesondere die schädliche Einwirkung des Mediums – sind für die Erzielung eines positiven Effektes zu nutzen.</od:description>
    <od:hasPrincipleNumber>22</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip der Umwandlung von Schädlichem in Nützliches</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P23">
    <od:description xml:lang="de">Es ist eine Rückkopplung einzuführen.</od:description>
    <od:description xml:lang="de">Falls eine Rückkopplung vorhanden ist, ist sie zu verändern.</od:description>
    <od:hasPrincipleNumber>23</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip der Rückkopplung</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P24">
    <od:description xml:lang="de">Es ist ein Zwischenobjekt zu benutzen, das die Wirkung überträgt, weitergibt oder auf sich nimmt.</od:description>
    <od:description xml:lang="de">Zeitweilig ist an das Objekt ein anderes leicht zu entfernendes Objekt anzuschließen.</od:description>
    <od:hasPrincipleNumber>24</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip des Vermittlers</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P25">
    <od:description xml:lang="de">Abprodukte, z. B. Energie und Stoff sind zu nutzen.</od:description>
    <od:description xml:lang="de">Das Objekt soll sich selbst bedienen sowie Hilfs- und Reparaturmaßnahmen selbst ausführen.</od:description>
    <od:hasPrincipleNumber>25</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip der Selbstbedienung</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P26">
    <od:description xml:lang="de">Anstelle eines unzugänglichen, komplizierten, kostspieligen, schlecht handhabbaren oder zerbrechlichen Objektes sind vereinfachte und billige Kopien zu benutzen.</od:description>
    <od:description xml:lang="de">Das Objekt oder das System von Objekten ist durch seine optischen Kopien (Abbildungen) zu ersetzen.</od:description>
    <od:hasPrincipleNumber>26</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip des Kopierens</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P27">
    <od:description xml:lang="de">Das teure Objekt ist durch ein Sortiment billiger Objekte zu ersetzen, wobei auf einige Qualitätseigenschaften, z. B. Langlebigkeit, verzichtet wird.</od:description>
    <od:hasPrincipleNumber>27</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip der billigen Kurzlebigkeit anstelle teurer Langlebigkeit</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P28">
    <od:description xml:lang="de">Die Felder sind in Kombination mit Ferromangetteilchen zu benutzen.</od:description>
    <od:description xml:lang="de">Elektrische, magnetische bzw. elektromagnetische Felder sind für eine Wechselwirkung mit dem Objekt auszunutzen.</od:description>
    <od:description xml:lang="de">Von unbeweglichen Feldern ist zu bewegten Feldern, von konstanten zu veränderlichen, von strukturlosen zu strukturierten Felder überzugehen.</od:description>
    <od:hasPrincipleNumber>28</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip des Ersatzes mechanischer Wirkprinzipien</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P29">
    <od:description xml:lang="de">Anstatt der schweren Teile des Objektes sind gasförmige oder flüssige zu benutzen, wie aufgeblasene oder mit Flüssigkeit gefüllte Teile, Luftkissen, hydrostatische oder hydroreaktive Teile.</od:description>
    <od:hasPrincipleNumber>29</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip der Anwendung von Pneumo- und Hydrokonstruktionen</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P30">
    <od:description xml:lang="de">Anstelle der üblichen Konstruktionen sind biegsame Hüllen und dünne Folien zu benutzen.</od:description>
    <od:description xml:lang="de">Das Objekt ist mit Hilfe biegsamer Hüllen und dünner Folie vom umgebenden Medium zu isolieren.</od:description>
    <od:hasPrincipleNumber>30</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip der Anwendung biegsamer Hüllen und dünner Folien</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P31">
    <od:description xml:lang="de">Das Objekt ist porös auszuführen, oder es sind zusätzlich poröse Elemente (Einsatzstücke, Überzüge usw.) zu benutzen.</od:description>
    <od:description xml:lang="de">Wenn das Objekt bereits porös ausgeführt ist, sind die Poren mit einem geeigneten Stoff zu füllen.</od:description>
    <od:hasPrincipleNumber>31</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip der Verwendung poröser Werkstoffe</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P32">
    <od:description xml:lang="de">Der Grad der Durchsichtigkeit des Objektes oder des umgebenden Mediums ist zu verändern.</od:description>
    <od:description xml:lang="de">Die Farbe des Objektes oder des umgebenden Mediums ist zu verändern.</od:description>
    <od:description xml:lang="de">Wenn solche Zustände bereits angewandt werden, sind Leuchtstoffe zu verwenden.</od:description>
    <od:description xml:lang="de">Zur Beobachtung schlecht sichtbarer Objekte oder Prozesse sind färbende Zuschläge zu nutzen.</od:description>
    <od:hasPrincipleNumber>32</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip der Farbveränderung</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P33">
    <od:description xml:lang="de">Objekte, die mit dem gegebenen Objekt zusammenwirken, müssen aus demselben Werkstoff oder einem Werkstoff mit annähernd gleichen Eigenschaften gefertigt sein.</od:description>
    <od:hasPrincipleNumber>33</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip der Gleichartigkeit bzw. Homogenität</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P34">
    <od:description xml:lang="de">Teil eines Objektes, das seinen Zweck erfüllt hat oder unbrauchbar geworden ist, wird beseitigt (aufgelöst, verdampft o. ä.) oder unmittelbar im Arbeitsgang umgewandelt.</od:description>
    <od:description xml:lang="de">Verbrauchte Teile eines Objektes werden unmittelbar im Arbeitsgang wieder hergestellt.</od:description>
    <od:hasPrincipleNumber>34</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip der Beseitigung und Regenerierung von Teilen</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P35">
    <od:description xml:lang="de">Hierzu gehören nicht nur einfache Übergänge, z. B. vom festen in den flüssigen Zustand, sondern auch die Übergänge in „Pseudo- oder Quasizustände“, z. B. die Quasiflüssigkeit und in Zwischenzustände, z. B. die Verwendung elastisch fester Körper.</od:description>
    <od:hasPrincipleNumber>35</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip der Veränderung des Aggregatzustandes</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P36">
    <od:description xml:lang="de">Die bei Phasenübergängen auftretenden Erscheinungen sind auszunutzen, z. B. Volumenveränderung, Wärmeentwicklung oder -absorption usw.</od:description>
    <od:hasPrincipleNumber>36</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip der Anwendung von Phasenübergängen</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P37">
    <od:description xml:lang="de">Die Wärmedehnung oder -verdichtung von Werkstoffen ist auszunutzen.</od:description>
    <od:description xml:lang="de">Es sind mehrere Werkstoffe mit unterschiedlicher Wärmedehnungszahl zu benutzen.</od:description>
    <od:hasPrincipleNumber>37</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip der Anwendung von Wärmedehnung</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P38">
    <od:description xml:lang="de">Die Luft oder der Sauerstoff ist der Einwirkung ionisierender Strahlung auszusetzen.</od:description>
    <od:description xml:lang="de">Die aktivierte Luft ist durch Sauerstoff zu ersetzen.</od:description>
    <od:description xml:lang="de">Die normale atmosphärische Luft ist durch aktivierte zu ersetzen.</od:description>
    <od:description xml:lang="de">Es ist ozonierter Sauerstoff zu benutzen.</od:description>
    <od:description xml:lang="de">Ozonierter oder ionisierter Sauerstoff ist durch Ozon zu ersetzen.</od:description>
    <od:hasPrincipleNumber>38</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip der Anwendung starker Oxidationsmittel</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P39">
    <od:description xml:lang="de">Das übliche Medium ist durch ein reaktionsträgeres zu ersehtzen.</od:description>
    <od:description xml:lang="de">Der Prozess ist im Vakuum durchzuführen.</od:description>
    <od:hasPrincipleNumber>39</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip der Verwendung eines inerten Mediums</rdfs:label>
  </od:Principle>
  <od:Principle rdf:about="http://opendiscovery.org/rdf/Principle/P40">
    <od:description xml:lang="de">Von gleichen Stoffen ist zu zusammengesetzten Stoffen überzugehen.</od:description>
    <od:hasPrincipleNumber>40</od:hasPrincipleNumber>
    <rdfs:label xml:lang="de">Prinzip der Anwendung zusammengesetzter Stoffe</rdfs:label>
  </od:Principle>
