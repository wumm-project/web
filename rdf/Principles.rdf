    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="de">Prinzip des Vermittlers</skos:prefLabel>
    <skos:prefLabel xml:lang="en">principle of the mediator</skos:prefLabel>
