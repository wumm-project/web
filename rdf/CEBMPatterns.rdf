  <od:CEBM rdf:about="http://opendiscovery.org/rdf/BusinessModel/LFG_P1">
    <od:hasCostsVC>reduction of inefficiencies</od:hasCostsVC>
    <od:hasCostsVC>reduction of waste disposal costs</od:hasCostsVC>
    <od:hasCustomerVD>manufacturers</od:hasCustomerVD>
    <od:hasExample>Royal DSM</od:hasExample>
    <od:hasLFId>P1</od:hasLFId>
    <od:hasProcessVC>recapturing of waste</od:hasProcessVC>
    <od:hasProcessVC>reusing of waste</od:hasProcessVC>
    <od:hasProcessVC>using own waste as input</od:hasProcessVC>
    <od:hasProcessVC>using waste of third parties as inputs</od:hasProcessVC>
    <od:hasProcessVD>replacing virgin materials and "linear" inputs</od:hasProcessVD>
    <od:hasProductVP>fully renewable, recyclable, or biodegradable inputs</od:hasProductVP>
    <od:hasProductVP>recycled inputs</od:hasProductVP>
    <skos:prefLabel>Circular supplies</skos:prefLabel>
  </od:CEBM>
  <od:CEBM rdf:about="http://opendiscovery.org/rdf/BusinessModel/LFG_P10">
    <od:hasCustomerVD>waste suppliers</od:hasCustomerVD>
    <od:hasCustomerVD>waste users</od:hasCustomerVD>
    <od:hasExample>smileexchange.ie (Ireland)</od:hasExample>
    <od:hasExample>thewastetradecompan y.co.za. (South Africa)</od:hasExample>
    <od:hasExample>wastetrade.it (Italy)</od:hasExample>
    <od:hasLFId>P10</od:hasLFId>
    <od:hasProcessVC>identification of gaps between demand and supply of waste</od:hasProcessVC>
    <od:hasProcessVD>online-based matching between waste suppliers and users</od:hasProcessVD>
    <od:hasRevenueVC>transaction fees</od:hasRevenueVC>
    <od:hasServiceVP>bringing together producers and users of waste</od:hasServiceVP>
    <skos:prefLabel>Online waste exchange platform</skos:prefLabel>
  </od:CEBM>
  <od:CEBM rdf:about="http://opendiscovery.org/rdf/BusinessModel/LFG_P11">
    <od:hasCustomerVD>end users buying remarketed products</od:hasCustomerVD>
    <od:hasCustomerVD>users of industrial equipment</od:hasCustomerVD>
    <od:hasExample>Appropriate for most capital-intensive B2B segments</od:hasExample>
    <od:hasExample>B2C companies serving markets where “reproducts commerce” is common</od:hasExample>
    <od:hasExample>Google’s project "Ara"</od:hasExample>
    <od:hasLFId>P11</od:hasLFId>
