<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF xmlns:cc="http://creativecommons.org/ns#"
   xmlns:dcterms="http://purl.org/dc/terms/"
   xmlns:od="http://opendiscovery.org/rdf/Model#"
   xmlns:owl="http://www.w3.org/2002/07/owl#"
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
   xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
   xmlns:skos="http://www.w3.org/2004/02/skos/core#"
   xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
   xmlns="http://opendiscovery.org/rdf/Glossary/">
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/40_inventive_principles">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">last collection of Inventive Principles authorised by G. Altshuller as published in (Altshuller,Sludyak,Rodman 1997)</skos:definition>
    <skos:definition xml:lang="de">letzte von G. Altshuller autorisierte Sammlung von Erfindungsgrundsätzen, wie in (Altshuller, Sludyak, Rodman 1997) veröffentlicht</skos:definition>
    <skos:definition xml:lang="ru">последняя коллекция изобретательских принципов, утвержденных Г. Альтшуллером, как опубликовано в (Altshuller, Sludyak, Rodman 1997)</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">40 erfinderische Prinzipien</skos:prefLabel>
    <skos:prefLabel xml:lang="en">40 inventive principles</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">40 изобретательских принципов</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/administrative_contradiction">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Problemsituation, in der weder ein Widerspruch noch eine Lösung erkennbar ist</skos:definition>
    <skos:definition xml:lang="en">problem situation in which neither a contradiction nor a solution is evident</skos:definition>
    <skos:definition xml:lang="ru">проблемная ситуация, в которой не видно ни противоречия, ни решения</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="en">administrative contradiction</skos:prefLabel>
    <skos:prefLabel xml:lang="de">administrativer Widerspruch</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">административное противоречие</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/algorithm_of_inventive_problem_solving">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Algorithmus zur Lösung eines Problems mit verschiedenen TRIZ-Tools, der das Problem durch eine Neuformulierung in mehreren Schritten verstärkt</skos:definition>
    <skos:definition xml:lang="en">algorithm for the solution of a problem with various TRIZ tools which intensifies the problem by reformulating it in a series of steps</skos:definition>
    <skos:definition xml:lang="ru">алгоритм решения проблемы с помощью различных инструментов ТРИЗ, который усиливает проблему, переформулируя ее в несколько этапов</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">Algorithmus zur erfinderischen Problemlösung (ARIZ)</skos:prefLabel>
    <skos:prefLabel xml:lang="en">algorithm of inventive problem solving (ARIZ)</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">алгоритм решения изобретательских задач (АРИЗ)</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/altshuller_matrix">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Widerspruchsmatrix mit 39 x 39 verallgemeinerten technischen Parametern</skos:definition>
    <skos:definition xml:lang="en">contradiction matrix with 39 x 39 generalised technical parameters</skos:definition>
    <skos:definition xml:lang="ru">матрица противоречий с обобщенными техническими параметрами 39 x 39</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:note xml:lang="en">Last contradiction matrix authorised by G. Altshuller.</skos:note>
    <skos:note xml:lang="de">Letzte von G. Altshuller autorisierte Widerspruchsmatrix.</skos:note>
    <skos:note xml:lang="ru">Последняя матрица противоречий, утвержденная Г. Альтшуллером.</skos:note>
    <skos:prefLabel xml:lang="en">Altshuller matrix</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Altshuller-Matrix</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Матрица Альтшуллера</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/anticipatory_failure_determination">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">TRIZ method for identifying possible system failures, their causes and ways for failure elimination, which also includes failures unobserved so far, future failures, and failures with unknown causes</skos:definition>
    <skos:definition xml:lang="de">TRIZ-Methode zur Identifizierung möglicher Systemfehler, ihrer Ursachen und Möglichkeiten zur Fehlerbeseitigung, einschließlich bisher nicht beobachteter Fehler, zukünftiger Fehler und Fehler mit unbekannten Ursachen </skos:definition>
    <skos:definition xml:lang="ru">Метод ТРИЗ для определения возможных сбоев системы, их причин и способов устранения сбоев, который также включает в себя ненаблюдаемые до настоящего времени сбои, будущие сбои и сбои с неизвестными причинами </skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">Vorausschauende Fehlerbestimmung (AFD)</skos:prefLabel>
    <skos:prefLabel xml:lang="en">anticipatory failure determination (AFD)</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Определение ожидаемого сбоя (AFD)</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/ariz-85c">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">last variant of the algorithm of inventive problem solving authorised by G. Altshuller dated 1985</skos:definition>
    <skos:definition xml:lang="de">letzte Variante des Algorithmus zur erfinderischen Problemlösung, autorisiert von G. Altshuller vom 1985</skos:definition>
    <skos:definition xml:lang="ru">последний вариант алгоритма решения изобретательских задач, утвержденный Г. Альтшуллером от 1985 г.</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">ariz-85c</skos:prefLabel>
    <skos:prefLabel xml:lang="en">ariz-85c</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">ariz-85c</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/catalogue_of_effects">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Katalog physikalischer, chemischer, geometrischer, biologischer oder anderer Effekte, die ausgewählt und angewendet werden können, um physikalische Widersprüche zu lösen und Funktionen zu erfüllen</skos:definition>
    <skos:definition xml:lang="en">catalogue of physical, chemical, geometrical, biological, or other effects which may be selected and applied to solve physical contradictions and to fulfil functions</skos:definition>
    <skos:definition xml:lang="ru">Каталог физических, химических, геометрических, биологических или других эффектов, которые могут быть выбраны и применены для разрешения физических противоречий и выполнения функций</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">Katalog der Effekte</skos:prefLabel>
    <skos:prefLabel xml:lang="en">catalogue of effects</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">каталог эффектов</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/cause-effect-chains_analysis">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Analysemethode, mit der die grundlegenden Ursachen für die Nachteile eines technischen Systems über Ursache-Wirkungsketten ermittelt werden können</skos:definition>
    <skos:definition xml:lang="en">analysis method which permits identifying the basic causes for disadvantages of a technical system via cause-effect chains</skos:definition>
    <skos:definition xml:lang="ru">метод анализа, позволяющий выявить основные причины недостатков технической системы с помощью причинно-следственных цепочек</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">Ursachen-Wirkungsketten-Analyse (CECA)</skos:prefLabel>
    <skos:prefLabel xml:lang="en">cause-effect-chains analysis (CECA)</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">анализ причинно-следственных связей (CECA)</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/cause-effect_diagram">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Systemmodell, das Ursache-Wirkungs-Beziehungen zwischen Funktionen, Komponenten, Ereignissen und Parametern des Systems und seiner Umgebung grafisch darstellt</skos:definition>
    <skos:definition xml:lang="en">system model which graphically represents cause-effect relations between functions, components, events, and parameters of the system and its environment</skos:definition>
    <skos:definition xml:lang="ru">системная модель, которая графически представляет причинно-следственные связи между функциями, компонентами, событиями и параметрами системы и ее среды</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">Ursache-Wirkungs-Diagramm (Ursache-Wirkungs-Ketten-Diagramm)</skos:prefLabel>
    <skos:prefLabel xml:lang="en">cause-effect diagram (cause-effect-chain diagram)</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">причинно-следственная схема (причинно-следственная схема)</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/classical_triz">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">TRIZ methods and tools by G. Altshuller. founder of TRIZ</skos:definition>
    <skos:definition xml:lang="de">TRIZ-Methoden und -Tools von G. Altshuller. Gründer der TRIZ</skos:definition>
    <skos:definition xml:lang="ru">Методы и инструменты ТРИЗ Г. Альтшуллера. основатель ТРИЗ</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">classical triz</skos:prefLabel>
    <skos:prefLabel xml:lang="en">classical triz</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">классический триз</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/contradiction">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Situation, in der zwei gegensätzliche Anforderungen an ein System zu erfüllen sind</skos:definition>
    <skos:definition xml:lang="en">situation of which two contrary demands to a system have to be met</skos:definition>
    <skos:definition xml:lang="ru">ситуация, когда необходимо выполнить два противоположных требования к системе</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:note xml:lang="en">TRIZ assumes that inventory problems use to be caused by contradictions. TRIZ knows three types of contradictions: &lt;ul&gt;&lt;li&gt;administrative&lt;/li&gt; &lt;li&gt;engineering&lt;/li&gt; &lt;li&gt;physical&lt;/li&gt;&lt;/ul&gt;</skos:note>
    <skos:note xml:lang="de">TRIZ geht davon aus, dass Inventarprobleme durch Widersprüche verursacht wurden. TRIZ kennt drei Arten von Widersprüchen: &lt;ul&gt; &lt;li&gt; administrative &lt;/ li&gt; &lt;li&gt; Engineering &lt;/ li&gt; &lt;li&gt; physisch &lt;/ li&gt; &lt;/ ul&gt; </skos:note>
    <skos:note xml:lang="ru">ТРИЗ предполагает, что проблемы инвентаризации возникают из-за противоречий. ТРИЗ знает три типа противоречий: &lt;ul&gt; &lt;li&gt; административный &lt;/li&gt; &lt;li&gt; инженерия &lt;/li&gt; &lt;li&gt; физическая &lt;/li&gt; &lt;/ul&gt; </skos:note>
    <skos:prefLabel xml:lang="de">Widerspruch</skos:prefLabel>
    <skos:prefLabel xml:lang="en">contradiction</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">противоречие</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/contradiction_matrix">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Tabelle, die eine Auswahl von erfinderischen Prinzipien für technische Widersprüche zwischen allgemeinen technischen Parametern empfiehlt</skos:definition>
    <skos:definition xml:lang="en">table which recommends a selection of inventive principles to engineering contradictions between common generalised engineering parameters</skos:definition>
    <skos:definition xml:lang="ru">таблица, которая рекомендует выбор изобретательских принципов для инженерных противоречий между общими обобщенными инженерными параметрами</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">Widerspruchsmatrix</skos:prefLabel>
    <skos:prefLabel xml:lang="en">contradiction matrix</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">матрица противоречий</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/disadvantage">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">undesired property of a technical system or process</skos:definition>
    <skos:definition xml:lang="de">unerwünschte Eigenschaft eines technischen Systems oder Prozesses</skos:definition>
    <skos:definition xml:lang="ru">нежелательное свойство технической системы или процесса</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">Nachteil</skos:prefLabel>
    <skos:prefLabel xml:lang="en">disadvantage</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">недостаток</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/engineering_contradiction">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Situation, bei der die Lösung des Problems einen Parameter (ersten Zielparameter) verbessern soll, aber einen anderen Parameter (zweiten Zielparameter) verschlechtern soll das gleiche System </skos:definition>
    <skos:definition xml:lang="en">situation at which solution of the problem one parameter (first target parameter) is to be improved but worsening another parameter (second target parameter) of the same system</skos:definition>
    <skos:definition xml:lang="ru">ситуация, при которой решение задачи одного параметра (первого целевого параметра) должно быть улучшено, но ухудшается другой параметр (второй целевой параметр) та же система </skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="en">engineering contradiction</skos:prefLabel>
    <skos:prefLabel xml:lang="de">technischer Widerspruch</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">инженерное противоречие</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/engineering_system">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Zusammenstellung mehrerer zusammenwirkender Elemente, die einen Zweck erfüllen</skos:definition>
    <skos:definition xml:lang="en">man-made assembly of several interacting elements which meets a purpose</skos:definition>
    <skos:definition xml:lang="ru">искусственная сборка из нескольких взаимодействующих элементов, которая соответствует цели</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">engineering system</skos:prefLabel>
    <skos:prefLabel xml:lang="en">engineering system</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">инженерная система</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/feature_transfer">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">TRIZ method for improving a technical system which uses transferring properties, characteristics, or features from an alternative technical system to the one examined</skos:definition>
    <skos:definition xml:lang="de">TRIZ-Verfahren zur Verbesserung eines technischen Systems, das die Übertragung von Eigenschaften, Merkmalen oder Merkmalen eines alternativen technischen Systems auf das untersuchte verwendet</skos:definition>
    <skos:definition xml:lang="ru">Метод ТРИЗ для улучшения технической системы, которая использует передачу свойств, характеристик или характеристик из альтернативной технической системы в исследуемую</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">feature transfer</skos:prefLabel>
    <skos:prefLabel xml:lang="en">feature transfer</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">передача функций</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/field">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Auswirkung auf ein Objekt, das die Eigenschaften des Objekts beeinflusst, d. h. ändert oder beibehält</skos:definition>
    <skos:definition xml:lang="en">effect onto an object which influences, i.e. changes or maintains, properties of the object</skos:definition>
    <skos:definition xml:lang="ru">воздействовать на объект, который влияет, то есть изменяет или поддерживает свойства объекта</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:note xml:lang="en">Commonly, five ciasses of effects are distinguished:&lt;ul&gt;&lt;li&gt;mechanical&lt;/li&gt; &lt;li&gt;acoustical&lt;/li&gt; &lt;li&gt;thermal&lt;/li&gt; &lt;li&gt;chemical&lt;/li&gt; &lt;li&gt;electro-magnetic&lt;/ul&gt;.</skos:note>
    <skos:note xml:lang="de">Die Definition dieses Begriffs unterscheidet sich von seiner Verwendung in der Physik als "Gebiet, in dem Kräfte wirken".</skos:note>
    <skos:note xml:lang="de">Im Allgemeinen werden fünf Kategorien von Effekten unterschieden: &lt;ul&gt; &lt;li&gt; mechanisch &lt;/li&gt; &lt;li&gt; akustisch &lt;/li&gt; &lt;li&gt; thermisch &lt;/li&gt; &lt;li&gt; chemisch &lt;/li&gt; &lt;li&gt; elektromagnetisch &lt;/li&gt;. </skos:note>
    <skos:note xml:lang="en">The definition of this term differs from its usage in physics as «area in which forces are acting».</skos:note>
    <skos:note xml:lang="ru">Обычно различают пять групп эффектов: &lt;ul&gt; &lt;li&gt; механический &lt;/li&gt; &lt;li&gt; акустический &lt;/li&gt; &lt;li&gt; термический &lt;/li&gt; &lt;li&gt; химический &lt;/li&gt; &lt;li&gt; электромагнитный &lt;/ul&gt;. </skos:note>
    <skos:note xml:lang="ru">Определение этого термина отличается от его использования в физике как "область, в которой действуют силы действующий".</skos:note>
    <skos:prefLabel xml:lang="de">field</skos:prefLabel>
    <skos:prefLabel xml:lang="en">field</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">поле</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/function">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Auswirkung eines Systems oder einer Systemkomponente auf eine oder mehrere andere, durch die ein Parameter der anderen Komponente oder des anderen Systems geändert, beseitigt oder beibehalten wird </skos:definition>
    <skos:definition xml:lang="en">effect from a system or a system component upon one or more others which changes, eliminates, or maintains a parameter of the other component or system</skos:definition>
    <skos:definition xml:lang="ru">эффект от системы или системного компонента на один или несколько других, который изменяет, исключает или поддерживает параметр другого компонента или системы </skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">function</skos:prefLabel>
    <skos:prefLabel xml:lang="en">function</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">функция</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/function_analysis">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">TRIZ method for the analysis of a system or its environment by means of a function model</skos:definition>
    <skos:definition xml:lang="de">TRIZ-Methode zur Analyse eines Systems oder seiner Umgebung mittels eines Funktionsmodells</skos:definition>
    <skos:definition xml:lang="ru">Метод ТРИЗ для анализа системы или ее среды с помощью функциональной модели</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">Funktionsanalyse</skos:prefLabel>
    <skos:prefLabel xml:lang="en">function analysis</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">анализ функций</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/function_model">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Modell eines technischen Systems, das die Komponenten des technischen Systems und der Systemumgebung zusammen mit ihren funktionalen Beziehungen grafisch, tabellarisch darstellt, oder andere Formen Gesetze der Entwicklung technischer Systeme allgemeine Muster, die die Entwicklung aller Arten technischer Systeme beschreiben </skos:definition>
    <skos:definition xml:lang="en">model of a technical system which represents the components of the technical system and of the system's environment together with their functional relations in graphical, tabular, or other forms laws of engineering systems evolution general patterns which describe the evolution of all kinds of technical systems</skos:definition>
    <skos:definition xml:lang="ru">модель технической системы, которая представляет компоненты технической системы и среды системы вместе с их функциональными отношениями в графическом, табличном, или другие формы законов общих схем эволюции инженерных систем, которые описывают эволюцию всех видов технических систем </skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:note xml:lang="de">Die Muster wurden ursprünglich von G. Altshuller entworfen. Erweiterungen und Detaillierungen werden auch als Muster, Trends und Linien der technischen Evolution bezeichnet.</skos:note>
    <skos:note xml:lang="en">The patterns were originally conceived by G. Altshuller. Extensions and detailings are also known as patterns, trends, and lins of technical evolution.</skos:note>
    <skos:note xml:lang="ru">Паттерны были изначально задуманы Г. Альтшуллером. Расширения и детализации также известны как паттерны, тренды и линии технической эволюции.</skos:note>
    <skos:prefLabel xml:lang="de">Funktionsmodell</skos:prefLabel>
    <skos:prefLabel xml:lang="en">function model</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">функциональная модель</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/ideal_final_result">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Modell der besten Lösung eines Inventarproblems</skos:definition>
    <skos:definition xml:lang="en">model of the best solution of an inventory problem</skos:definition>
    <skos:definition xml:lang="ru">модель наилучшего решения проблемы инвентаризации</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:note xml:lang="de">Das ideale Endergebnis löst das Problem vollständig mit minimalen Änderungen des Systems und ohne Verschlechterung der Systemeigenschaften.</skos:note>
    <skos:note xml:lang="en">The ideal final result completely solves the problem with minimal changes of the system and without deterioration of system properties.</skos:note>
    <skos:note xml:lang="ru">Идеальный конечный результат полностью решает проблему с минимальными изменениями системы и без ухудшения свойств системы.</skos:note>
    <skos:prefLabel xml:lang="de">Ideales Endresultat (IER)</skos:prefLabel>
    <skos:prefLabel xml:lang="en">ideal final result (ifr)</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">идеальный конечный результат (ifr)</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/ideal_technical_system">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">technical system which fulfils the main useful function without existing by itself and without any harmful side effects</skos:definition>
    <skos:definition xml:lang="de">technisches System, das die Hauptnutzfunktion erfüllt, ohne für sich zu existieren und ohne schädliche Nebenwirkungen</skos:definition>
    <skos:definition xml:lang="ru">техническая система, которая выполняет основную полезную функцию, не существуя сама по себе и без каких-либо вредных побочных эффектов</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:note xml:lang="en">If the technical system is a machine, it is also called «ideal machine».</skos:note>
    <skos:note xml:lang="de">Wenn das technische System eine Maschine ist, wird es auch als "ideale Maschine" bezeichnet.</skos:note>
    <skos:note xml:lang="ru">Если техническая система представляет собой машину, ее также называют "идеальной машиной".</skos:note>
    <skos:prefLabel xml:lang="de">Ideales technisches System</skos:prefLabel>
    <skos:prefLabel xml:lang="en">ideal technical system</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">идеальная техническая система</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/ideality">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">dimensionless ratio of benefit and expenses</skos:definition>
    <skos:definition xml:lang="de">dimensionsloses Verhältnis von Nutzen und Kosten</skos:definition>
    <skos:definition xml:lang="ru">безразмерное соотношение выгод и расходов</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:note xml:lang="de">Die Ausgaben umfassen unerwünschte Effekte, Kosten, Probleme oder andere Faktoren.</skos:note>
    <skos:note xml:lang="en">The expenses comprise undesired effects, costs, problems or other factors.</skos:note>
    <skos:note xml:lang="ru">Расходы включают нежелательные эффекты, затраты, проблемы или другие факторы.</skos:note>
    <skos:prefLabel xml:lang="de">Idealität</skos:prefLabel>
    <skos:prefLabel xml:lang="en">ideality</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">идеальность</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/innovation_checklist">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Sammlung von Fragen und Richtlinien, die helfen, die Situation des Innovationsproblems zu analysieren</skos:definition>
    <skos:definition xml:lang="en">collection of questions and directives which help to analyse the situation of the innovation problem</skos:definition>
    <skos:definition xml:lang="ru">сборник вопросов и директив, помогающих проанализировать ситуацию с инновационной проблемой</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:note xml:lang="de">Bezogen auf den Fragebogen zur Innovationssituation ^ TM erfinderische Prinzipien</skos:note>
    <skos:note xml:lang="en">Related to Innovation Situation Questionnaire^TM inventive principles</skos:note>
    <skos:note xml:lang="ru">Относительно вопросника по инновационной ситуации изобретательские принципы</skos:note>
    <skos:prefLabel xml:lang="de">Innovationscheckliste</skos:prefLabel>
    <skos:prefLabel xml:lang="en">innovation checklist</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">контрольный список инноваций</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/inventive_principles">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Grundlegende Lösungen für technische Probleme</skos:definition>
    <skos:definition xml:lang="en">basic solutions for technical problems</skos:definition>
    <skos:definition xml:lang="ru">основные решения технических проблем</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:note xml:lang="de">Die Prinzipien wurden aus der Analyse bestehender Patente abgeleitet.</skos:note>
    <skos:note xml:lang="en">The principles were derived from the analysis of existing patents.</skos:note>
    <skos:note xml:lang="ru">Принципы были получены на основе анализа существующих патентов.</skos:note>
    <skos:prefLabel xml:lang="de">erfinderische Prinzipien</skos:prefLabel>
    <skos:prefLabel xml:lang="en">inventive principles</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">изобретательские принципы</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/inventive_problem">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Problem, das die Konzeption eines vollständig oder teilweise neuen Systems erfordert, um eine Struktur zu implementieren oder eine Funktion zu erfüllen</skos:definition>
    <skos:definition xml:lang="en">problem requiring the conception of a completely or partially new system in order to implement a structure or fulfil a function</skos:definition>
    <skos:definition xml:lang="ru">проблема, требующая концепции полностью или частично новой системы для реализации структуры или выполнения функции</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:note xml:lang="de">Das System muss nach dem Stand der Wissenschaft neu sein und Technologie. </skos:note>
    <skos:note xml:lang="de">Die Konzeption enthält eine erfinderische Tätigkeit.</skos:note>
    <skos:note xml:lang="en">The conception contains an inventive step or activity.</skos:note>
    <skos:note xml:lang="en">The system must be new regarding the known state of the art in science and technology.</skos:note>
    <skos:note xml:lang="ru">Концепция содержит изобретательский уровень или действие.</skos:note>
    <skos:note xml:lang="ru">Система должна быть новой в отношении известного уровня техники в науке и технологии. </skos:note>
    <skos:prefLabel xml:lang="de">erfinderisches Problem</skos:prefLabel>
    <skos:prefLabel xml:lang="en">inventive problem</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">изобретательская проблема</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/inventive_problem_solving">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Problemlösungsprozess, der neuartige Lösungen hervorbringt, die für den Fachmann nach dem Stand der Technik nicht erkennbar sind und die als Neuheit angesehen werden</skos:definition>
    <skos:definition xml:lang="en">process of problem solving which produces novel solutions not apparent to the expert by the state of the art and which are considered as novelty</skos:definition>
    <skos:definition xml:lang="ru">процесс решения проблем, который приводит к новым решениям, не очевидным для специалиста в данной области техники и которые рассматриваются как новизна</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">erfinderische Problemlösung</skos:prefLabel>
    <skos:prefLabel xml:lang="en">inventive problem solving</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">решение изобретательских задач</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/inventive_standard">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Allgemeine Richtlinie zur Änderung einer Substanzfeldstruktur zur Problemlösung</skos:definition>
    <skos:definition xml:lang="en">general directive for the modification of a substance-field structure for problem solution</skos:definition>
    <skos:definition xml:lang="ru">общая директива для модификации структуры вещества-поля для решения проблемы</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">erfinderischer Standard</skos:prefLabel>
    <skos:prefLabel xml:lang="en">inventive standard</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">изобретательский стандарт</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/level_of_invention">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Kennzahl für die Beurteilung des intellektuellen Niveaus einer Erfindung im Vergleich zum Stand der Technik zum Zeitpunkt der Erfindung</skos:definition>
    <skos:definition xml:lang="en">characteristic number for the assessment of the intellectual level of an invention compared to the state of the art at the time of the invention</skos:definition>
    <skos:definition xml:lang="ru">характерное число для оценки интеллектуального уровня изобретения по сравнению с современным уровнем на момент изобретения</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">Erfindungsgrad</skos:prefLabel>
    <skos:prefLabel xml:lang="en">level of invention</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">уровень изобретательности</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/operator">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Direktive zur Erzeugung oder Spezifikation einer Lösung</skos:definition>
    <skos:definition xml:lang="en">directive for generation or specification of a solution</skos:definition>
    <skos:definition xml:lang="ru">директива для генерации или спецификации решения</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">operator</skos:prefLabel>
    <skos:prefLabel xml:lang="en">operator</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">оператор</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/physical_contradiction">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Situation, in der ein Parameter entgegengesetzte Werte annehmen muss, um widersprüchliche Anforderungen an ein System zu erfüllen</skos:definition>
    <skos:definition xml:lang="en">situation which requires a parameter to take opposing values in order to meet contradictory demands to a system</skos:definition>
    <skos:definition xml:lang="ru">ситуация, в которой требуется, чтобы параметр принимал противоположные значения для удовлетворения противоречивых требований к системе</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="en">physical contradiction</skos:prefLabel>
    <skos:prefLabel xml:lang="de">physischer Widerspruch</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">физическое противоречие</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/principles_of_separating_contradictory_demands">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Prinzipien zur Lösung physikalischer Widersprüche, die durch ihre Trennung den widersprüchlichen Anforderungen genügen</skos:definition>
    <skos:definition xml:lang="en">principles for the solution of physical contradictions which meet the contradictory demands by their separation</skos:definition>
    <skos:definition xml:lang="ru">принципы решения физических противоречий, которые удовлетворяют противоречивые требования путем их разделения</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">Prinzipien der Trennung widersprüchlicher Forderungen</skos:prefLabel>
    <skos:prefLabel xml:lang="en">principles of separating contradictory demands</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">принципы разделения противоречивых требований</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/problem_formulation">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">TRIZ method for analysing a system and its environment by means of a special cause-and-effect chain model</skos:definition>
    <skos:definition xml:lang="de">TRIZ-Methode zur Analyse eines Systems und seiner Umgebung mittels eines speziellen Ursache-Wirkungs-Kettenmodells</skos:definition>
    <skos:definition xml:lang="ru">Метод ТРИЗ для анализа системы и ее среды с помощью специальной модели причинно-следственной цепочки</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">Problem Formulierung</skos:prefLabel>
    <skos:prefLabel xml:lang="en">problem formulation</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">постановка проблемы</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/psychological_inertia">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Die Unfähigkeit einer Person, eine Lösung für ein Problem zu finden, das außerhalb ihrer mentalen Konzentration liegt und von persönlichen Erfahrungen, Kenntnissen, Assoziationen usw. betroffen ist.</skos:definition>
    <skos:definition xml:lang="en">a person's inability to find a solution to a problem outside of his mental focusing affected by personal experience, knowledge, associations, etc.</skos:definition>
    <skos:definition xml:lang="ru">неспособность человека найти решение проблемы вне его психической сосредоточенности, на которую влияют личный опыт, знания, ассоциации и т. д.</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="en">psychological inertia</skos:prefLabel>
    <skos:prefLabel xml:lang="de">psychologische Trägheit</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">психологическая инерция</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/resource">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">mean suitable for the solution of a problem</skos:definition>
    <skos:definition xml:lang="de">zur Lösung eines Problems geeignet</skos:definition>
    <skos:definition xml:lang="ru">означает подходящее для решения проблемы</skos:definition>
    <skos:example xml:lang="en">time, space, substance, information, and functions with their respective properties</skos:example>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">resource</skos:prefLabel>
    <skos:prefLabel xml:lang="en">resource</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">ресурс</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/resource_analysis">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Suche und Beschreibung der verfügbaren und entwickelbaren Ressourcen im System selbst und in seiner Umgebung</skos:definition>
    <skos:definition xml:lang="en">search for and description of available and developable resources in the system itself and its environment</skos:definition>
    <skos:definition xml:lang="ru">поиск и описание доступных и доступных ресурсов в самой системе и ее среде</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">Ressourcenanalyse</skos:prefLabel>
    <skos:prefLabel xml:lang="en">resource analysis</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">анализ ресурсов</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/root-conflict-analysis">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Ursachen-Wirkungsketten-Analyse, die die vollständige Identifizierung von Widersprüchen ermöglicht</skos:definition>
    <skos:definition xml:lang="en">cause-effect-chains analysis which permits complete identification of contradictions</skos:definition>
    <skos:definition xml:lang="ru">анализ причинно-следственных связей, который позволяет полностью выявить противоречия</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">root-conflict-analysis</skos:prefLabel>
    <skos:prefLabel xml:lang="en">root-conflict-analysis</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">анализ корневых конфликтов</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/s-curve_analysis">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Einschätzung der potenziellen zukünftigen Entwicklung eines Systems oder einer Technologie durch Bestimmung ihrer aktuellen Position und der S-Kurve der technischen Entwicklung</skos:definition>
    <skos:definition xml:lang="en">estimation of the potential future evolution of a system or a technology by determining its current position an the S curve of technical evolution</skos:definition>
    <skos:definition xml:lang="ru">оценка потенциального будущего развития системы или технологии путем определения ее текущего положения и S-кривой технического развития</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">s-curve analysis</skos:prefLabel>
    <skos:prefLabel xml:lang="en">s-curve analysis</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">анализ s-кривой</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/s-curve_of_technical_evolution">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">S-förmige Kurve, die den Trend eines nützlichen Systemparameters über die Zeit darstellt und typische Entwicklungsphasen aufweist</skos:definition>
    <skos:definition xml:lang="en">S-shaped curve which depicts the trend of a useful system parameter over time and features typical development phases</skos:definition>
    <skos:definition xml:lang="ru">S-образная кривая, которая отображает тенденцию изменения полезного параметра системы во времени и имеет типичные фазы разработки</skos:definition>
    <skos:example xml:lang="en">system creation, growth, maturity, and decline</skos:example>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="ru">S-образная кривая технического развития</skos:prefLabel>
    <skos:prefLabel xml:lang="de">s-curve of technical evolution</skos:prefLabel>
    <skos:prefLabel xml:lang="en">s-curve of technical evolution</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/size-time-cost_operator">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">TRIZ method for overcoming the inventor's psychological bias by means of a thought experiment which varies system properties related to dimensions, time, and tost to their extremes</skos:definition>
    <skos:definition xml:lang="de">TRIZ-Methode zur Überwindung der psychologischen Vorurteile des Erfinders durch ein Gedankenexperiment, das die Systemeigenschaften in Bezug auf Dimensionen, Zeit und Stärke variiert Extreme </skos:definition>
    <skos:definition xml:lang="ru">Метод ТРИЗ для преодоления психологического смещения изобретателя с помощью мысленного эксперимента, который изменяет свойства системы, связанные с размерами, временем и их соответствием крайности </skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">Größe-Zeit-Kosten-Operator</skos:prefLabel>
    <skos:prefLabel xml:lang="en">size-time-cost operator</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">оператор размера-времени-стоимости</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/smart-little-people_model">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Modell zur Systemanalyse und zur Ideenfindung, bei dem das System und mögliche Lösungen durch die Handlungen von Elementen (sogenannte kleine Leute, auch Zwerge genannt), die jede Fähigkeit besitzen und jedes gewünschte Verhalten ausführen können </skos:definition>
    <skos:definition xml:lang="en">model for system analysis and for idea creation in which the system and possible solutions are represented by the actions of elements (so called little people, also known as dwarfs) who can possess any ability and perform any desired behaviour</skos:definition>
    <skos:definition xml:lang="ru">модель для системного анализа и создания идеи, в которой система и возможные решения представлены действиями элементов (так называемые маленькие люди, также известный как гномы), который может обладать любой способностью и выполнять любое желаемое поведение </skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">smart-little-people model</skos:prefLabel>
    <skos:prefLabel xml:lang="en">smart-little-people model</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">модель умных маленьких людей</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/substance">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">mass-bound object</skos:definition>
    <skos:definition xml:lang="de">massengebundenes Objekt</skos:definition>
    <skos:definition xml:lang="ru">объект, связанный с массой</skos:definition>
    <skos:example xml:lang="en">gas, liquid, solid body, or plasma</skos:example>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">Substanz</skos:prefLabel>
    <skos:prefLabel xml:lang="en">substance</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">вещество</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/substance-field_analysis">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">abstract representation of a problem situation by means of a substance-field-model</skos:definition>
    <skos:definition xml:lang="de">abstrakte Darstellung einer Problemsituation mittels eines Substanzfeldmodells</skos:definition>
    <skos:definition xml:lang="ru">абстрактное представление проблемной ситуации с помощью модели вещества-поля</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">Substanzfeldanalyse</skos:prefLabel>
    <skos:prefLabel xml:lang="en">substance-field analysis</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">анализ поля вещества</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/substance-field_model">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">abstract graphical representation of a problem situation or a problem solution (e.g. many standard solutions) using substances and fields as elements</skos:definition>
    <skos:definition xml:lang="de">abstrakte grafische Darstellung einer Problemsituation oder einer Problemlösung (z. B. viele Standardlösungen) unter Verwendung von Substanzen und Feldern als Elemente</skos:definition>
    <skos:definition xml:lang="ru">абстрактное графическое представление проблемной ситуации или решения проблемы (например, множество стандартных решений) с использованием веществ и полей в качестве элементов</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">Substanzfeldmodell (su-Feld)</skos:prefLabel>
    <skos:prefLabel xml:lang="en">substance-field model (su-field)</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">модель поля вещества (su-field)</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/subsystem">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">subordinate system which is part of the examined system</skos:definition>
    <skos:definition xml:lang="de">untergeordnetes System, das Teil des untersuchten Systems ist</skos:definition>
    <skos:definition xml:lang="ru">подчиненная система, которая является частью рассматриваемой системы</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">subsystem</skos:prefLabel>
    <skos:prefLabel xml:lang="en">subsystem</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">подсистема</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/supersystem">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">superior system which contains the examined System as a component</skos:definition>
    <skos:definition xml:lang="de">übergeordnetes System, das das untersuchte System als Komponente enthält</skos:definition>
    <skos:definition xml:lang="ru">вышестоящая система, которая содержит исследуемую Систему как компонент</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">supersystem</skos:prefLabel>
    <skos:prefLabel xml:lang="en">supersystem</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">суперсистема</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/system_of_inventive_standards">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Klassifizierung von 76 Standardlösungen, gruppiert in fünf Klassen typischer Probleme</skos:definition>
    <skos:definition xml:lang="en">classification of 76 standard solutions, grouped into five classes of typical problems</skos:definition>
    <skos:definition xml:lang="ru">классификация 76 стандартных решений, сгруппированных в пять классов типичных проблем</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">System erfinderischer Standards</skos:prefLabel>
    <skos:prefLabel xml:lang="en">system of inventive standards</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">система изобретательских стандартов</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/system_operator">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Schema zur Systemanalyse oder zur Problemlösung und zur Abschätzung der zukünftigen Systementwicklung</skos:definition>
    <skos:definition xml:lang="en">scheme for system analysis or for problem solution and for estimation of future system evolution</skos:definition>
    <skos:definition xml:lang="ru">схема для системного анализа или для решения проблемы и для оценки будущего развития системы</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:note xml:lang="de">Das Schema besteht aus einer Tabelle, deren Spalten unterschiedliche Zeitpunkte und die Zeilen unterschiedliche Systemebenen darstellen (z. B. drei Spalten und drei Zeilen) in der Multi-Screen-Analyse mit neun Bildschirmen). </skos:note>
    <skos:note xml:lang="en">The scheme consists of a table the columns of which represent different points in time and the rows different system levels (e.g. three columns and three rows in multi-screen analysis with nine screens).</skos:note>
    <skos:note xml:lang="ru">Схема состоит из таблицы, столбцы которой представляют разные моменты времени и строки разных системных уровней (например, три столбца и три строки в многоэкранном анализе с девятью экранами). </skos:note>
    <skos:prefLabel xml:lang="de">system operator</skos:prefLabel>
    <skos:prefLabel xml:lang="en">system operator</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">системный оператор</skos:prefLabel>
  </od:GlossaryEntry>
  <skos:ConceptScheme rdf:about="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary">
    <skos:prefLabel xml:lang="de">Das TRIZ-Glossar</skos:prefLabel>
    <skos:prefLabel xml:lang="en">The TRIZ Glossary</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Глоссарий ТРИЗ</skos:prefLabel>
  </skos:ConceptScheme>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/trimming">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">TRIZ method for increasing ideality of a technical system by means of elimination of single components</skos:definition>
    <skos:definition xml:lang="de">TRIZ-Methode zur Steigerung der Idealität eines technischen Systems durch Wegfall einzelner Komponenten</skos:definition>
    <skos:definition xml:lang="ru">Метод ТРИЗ для повышения идеальности технической системы за счет исключения отдельных компонентов</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:note xml:lang="de">Die nützlichen Funktionen der eliminierten Komponenten werden anderen Komponenten des Systems oder seiner Supersysteme zugewiesen.</skos:note>
    <skos:note xml:lang="en">The useful functions of the eliminated components are reassigned to other components of the system or of its supersystems.</skos:note>
    <skos:note xml:lang="ru">Полезные функции исключенных компонентов переназначаются другим компонентам системы или ее суперсистем.</skos:note>
    <skos:prefLabel xml:lang="de">trimmen</skos:prefLabel>
    <skos:prefLabel xml:lang="en">trimming</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">обрезка</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/triz">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">scientific and application-oriented discipline which comprises studying directions of engineering systems evolution and the process of inventive problem solving in order to develop methods and tools to support innovative improvement of engineering systems based an a systematic and knowledge-based approach</skos:definition>
    <skos:definition xml:lang="de">wissenschaftliche und anwendungsorientierte Disziplin, die das Studium der Richtungen der Entwicklung von Ingenieurssystemen und des Prozesses der erfinderischen Problemlösung umfasst, um Methoden und Werkzeuge zu entwickeln Unterstützung der innovativen Verbesserung technischer Systeme auf der Grundlage eines systematischen und wissensbasierten Ansatzes.</skos:definition>
    <skos:definition xml:lang="ru">научная и прикладная дисциплина, которая включает в себя изучение направлений эволюции инженерных систем и процесса решения изобретательских задач с целью разработки методов и инструментов поддерживать инновационное совершенствование инженерных систем на основе системного и основанного на знаниях подхода </skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:note xml:lang="de">In den ersten Übersetzungen vom Russischen ins Englische wurde TRIZ als TIPS (theory of inventive problem solving) übersetzt. </skos:note>
    <skos:note xml:lang="de">In letzter Zeit wurde TRIZ auf nichttechnologische Bereiche ausgedehnt, z.B. wirtschaftliche, organisatorische oder informationstechnische Systeme.</skos:note>
    <skos:note xml:lang="en">In the first translations from Russian to English, TRIZ was translated as TIPS. The abbreviation "TIPS" is the acronym for "theory of inventive problem solving".</skos:note>
    <skos:note xml:lang="en">Lately, TRIZ has been extended to non-technology areas, e.g. economical, organisational or information-technical systems..</skos:note>
    <skos:note xml:lang="ru">В первых переводах с русского на английский ТРИЗ была переведена как TIPS. Аббревиатура "TIPS" является аббревиатурой от "theory of inventive problem solving".</skos:note>
    <skos:note xml:lang="ru">В последнее время ТРИЗ была распространена на нетехнологические области, например, на экономические, организационные или информационно-технические системы.</skos:note>
    <skos:prefLabel xml:lang="de">TRIZ (Theorie der erfinderischen Problemlösung)</skos:prefLabel>
    <skos:prefLabel xml:lang="en">TRIZ (theory of inventive problem solving, TIPS)</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">ТРИЗ (теория решения изобретательских задач)</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/triz_forecasting">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Verfahren zur Vorhersage der Entwicklung technischer Systeme mit TRIZ-Methoden</skos:definition>
    <skos:definition xml:lang="en">procedure to forecast the evolution of technical systems with TRIZ methods</skos:definition>
    <skos:definition xml:lang="ru">процедура прогнозирования развития технических систем с использованием методов ТРИЗ</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">triz forecasting</skos:prefLabel>
    <skos:prefLabel xml:lang="en">triz forecasting</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">прогнозирование триз</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/triz_method">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Art der Anwendung eines oder mehrerer TRIZ-Tools</skos:definition>
    <skos:definition xml:lang="en">way of application of one or more TRIZ tools</skos:definition>
    <skos:definition xml:lang="ru">способ применения одного или нескольких инструментов ТРИЗ</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">triz method</skos:prefLabel>
    <skos:prefLabel xml:lang="en">triz method</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">метод triz</skos:prefLabel>
  </od:GlossaryEntry>
  <od:GlossaryEntry rdf:about="http://opendiscovery.org/rdf/Glossary/triz_tool">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Element der TRIZ zur Bearbeitung von Inventarproblemen</skos:definition>
    <skos:definition xml:lang="en">element of TRIZ for processing inventory Problems</skos:definition>
    <skos:definition xml:lang="ru">элемент ТРИЗ для обработки проблем инвентаризации</skos:definition>
    <skos:inScheme rdf:resource="http://opendiscovery.org/rdf/Glossary/theTRIZGlossary"/>
    <skos:prefLabel xml:lang="de">triz tool</skos:prefLabel>
    <skos:prefLabel xml:lang="en">triz tool</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">инструмент triz</skos:prefLabel>
  </od:GlossaryEntry>
