<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF xmlns:cc="http://creativecommons.org/ns#"
   xmlns:dcterms="http://purl.org/dc/terms/"
   xmlns:od="http://opendiscovery.org/rdf/Model#"
   xmlns:owl="http://www.w3.org/2002/07/owl#"
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
   xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
   xmlns:skos="http://www.w3.org/2004/02/skos/core#"
   xmlns:tc="http://opendiscovery.org/rdf/Concept/"
   xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/Adaptability">
    <od:hasAltshuller71Id>35</od:hasAltshuller71Id>
    <od:hasAltshuller84Id>35</od:hasAltshuller84Id>
    <od:hasMatrix2003Id>32</od:hasMatrix2003Id>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">The extent to which a system or object is appropriate for all situations or positively responds to external changes.</skos:definition>
    <skos:prefLabel xml:lang="en">Adaptability, Universality</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Adaptionsfähigkeit, Universalität</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Адаптация, универсальность</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/AestheticsOrAppearance">
    <od:hasMatrix2003Id>39</od:hasMatrix2003Id>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Aesthetics or Appearance</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/AmountOfSubstance">
    <od:hasAltshuller71Id>26</od:hasAltshuller71Id>
    <od:hasAltshuller84Id>26</od:hasAltshuller84Id>
    <od:hasMatrix2003Id>10</od:hasMatrix2003Id>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">The number or amount of a system’s materials, substances, parts, or subsystems which are altered (permanently or temporarily) or consumed (fully or partially) during normal functioning.</skos:definition>
    <skos:prefLabel xml:lang="en">Amount of substance</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Materialmenge</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Количество вещества</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/AreaOfTheMovingObject">
    <od:hasAltshuller71Id>05</od:hasAltshuller71Id>
    <od:hasAltshuller84Id>05</od:hasAltshuller84Id>
    <od:hasMatrix2003Id>05</od:hasMatrix2003Id>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">The outermost or uppermost layer of the object.</skos:definition>
    <skos:prefLabel xml:lang="en">Area of the moving object</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Fläche des bewegten Objekts</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Площадь подвижного объекта</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/AreaOfTheStationaryObject">
    <od:hasAltshuller71Id>06</od:hasAltshuller71Id>
    <od:hasAltshuller84Id>06</od:hasAltshuller84Id>
    <od:hasMatrix2003Id>06</od:hasMatrix2003Id>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">The outermost or uppermost layer of the object.</skos:definition>
    <skos:prefLabel xml:lang="en">Area of the stationary object</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Fläche des unbewegten Objekts</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Площадь неподвижного объекта</skos:prefLabel>
  </od:Parameter>
  <od:ParameterClass rdf:about="http://opendiscovery.org/rdf/Concept/AssemblyCost">
    <od:contains rdf:resource="http://opendiscovery.org/rdf/Concept/ComplexityOfControl"/>
    <od:contains rdf:resource="http://opendiscovery.org/rdf/Concept/DeviceComplexity"/>
    <od:contains rdf:resource="http://opendiscovery.org/rdf/Concept/EaseOfManufacturing"/>
    <od:contains rdf:resource="http://opendiscovery.org/rdf/Concept/LevelOfAutomation"/>
    <od:contains rdf:resource="http://opendiscovery.org/rdf/Concept/ManufacturingPrecision"/>
    <od:contains rdf:resource="http://opendiscovery.org/rdf/Concept/Productivity"/>
    <skos:prefLabel xml:lang="en">Assembly Cost</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Fertigungskosten</skos:prefLabel>
  </od:ParameterClass>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/CompatitbilityOrConnectability">
    <od:hasMatrix2003Id>33</od:hasMatrix2003Id>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Compatitbility Or Connectability</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/ComplexityOfControl">
    <od:hasMatrix2003Id>46</od:hasMatrix2003Id>
    <skos:prefLabel xml:lang="en">Complexity Of Control</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/DeviceComplexity">
    <od:hasAltshuller71Id>36</od:hasAltshuller71Id>
    <od:hasAltshuller84Id>36</od:hasAltshuller84Id>
    <od:hasMatrix2003Id>45</od:hasMatrix2003Id>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:altLabel xml:lang="en">Complexity of the structure</skos:altLabel>
    <skos:altLabel xml:lang="de">Kompliziertheit der Struktur</skos:altLabel>
    <skos:definition xml:lang="en">The number and diversity of elements and element interrelationships comprising a system. Human interaction increases complexity.</skos:definition>
    <skos:prefLabel xml:lang="en">Device complexity</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Kompliziertheit des Geräts</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Сложность устройства</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/DifficultyOfDetecting">
    <od:hasAltshuller71Id>37</od:hasAltshuller71Id>
    <od:hasAltshuller84Id>37</od:hasAltshuller84Id>
    <od:hasMatrix2003Id>47</od:hasMatrix2003Id>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:altLabel xml:lang="en">Complexity of control and measurement</skos:altLabel>
    <skos:definition xml:lang="en">Demonstrated by measuring or monitoring systems that are complex, costly, require much time and labor to set up and use, or that have complex relationships between components or components that interfere with each other. Can also be indicated by the increasing cost of measuring to a satisfactory error.</skos:definition>
    <skos:prefLabel xml:lang="en">Difficulty of detecting</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Kompliziertheit der Kontrolle und Messbarkeit</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Сложность контроля и измерения</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/DurabilityOfTheMovingObject">
    <od:hasAltshuller71Id>15</od:hasAltshuller71Id>
    <od:hasAltshuller84Id>15</od:hasAltshuller84Id>
    <od:hasMatrix2003Id>12</od:hasMatrix2003Id>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">The time that the object or system can correctly perform its action. Also referred to as service life or durability.</skos:definition>
    <skos:prefLabel xml:lang="en">Durability of the moving object</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Haltbarkeit des bewegten Objekts</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Время действия подвижного объекта</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/DurabilityOfTheStationaryObject">
    <od:hasAltshuller71Id>16</od:hasAltshuller71Id>
    <od:hasAltshuller84Id>16</od:hasAltshuller84Id>
    <od:hasMatrix2003Id>13</od:hasMatrix2003Id>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">The time that the object or system can correctly perform its action. Also referred to as service life or durability.</skos:definition>
    <skos:prefLabel xml:lang="en">Durability of the stationary object</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Haltbarkeit des unbewegten Objekts</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Время действия неподвижного объекта</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/EaseOfManufacturing">
    <od:hasAltshuller71Id>32</od:hasAltshuller71Id>
    <od:hasAltshuller84Id>32</od:hasAltshuller84Id>
    <od:hasMatrix2003Id>41</od:hasMatrix2003Id>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:altLabel xml:lang="en">Convenience of manufacturing</skos:altLabel>
    <skos:definition xml:lang="en">The degree of facility or effortlessness in manufacturing or fabricating the object or system.</skos:definition>
    <skos:prefLabel xml:lang="en">Ease of manufacturing</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Fertigungsfreundlichkeit</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Удобство изготовления</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/EaseOfOperation">
    <od:hasAltshuller71Id>33</od:hasAltshuller71Id>
    <od:hasAltshuller84Id>33</od:hasAltshuller84Id>
    <od:hasMatrix2003Id>34</od:hasMatrix2003Id>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:altLabel xml:lang="en">Convenience of use</skos:altLabel>
    <skos:definition xml:lang="en">Simple, easy to understand and perform or utilize; accessible.</skos:definition>
    <skos:prefLabel xml:lang="de">Benutzerfreundlichkeit</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Ease of operation</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Удобство эксплуатации</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/EaseOfRepair">
    <od:hasAltshuller71Id>34</od:hasAltshuller71Id>
    <od:hasAltshuller84Id>34</od:hasAltshuller84Id>
    <od:hasMatrix2003Id>36</od:hasMatrix2003Id>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:altLabel xml:lang="en">Convenience of repair</skos:altLabel>
    <skos:definition xml:lang="en">The degree of facility or effortlessness in the mending or fixing of the system. Encompasses convenience as well as time to repair faults, failures, or defects.</skos:definition>
    <skos:prefLabel xml:lang="en">Ease of repair</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Reparaturfreundlichkeit</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Удобство ремонта</skos:prefLabel>
  </od:Parameter>
  <od:ParameterClass rdf:about="http://opendiscovery.org/rdf/Concept/Efficiency">
    <od:contains rdf:resource="http://opendiscovery.org/rdf/Concept/FunctionEfficiency"/>
    <od:contains rdf:resource="http://opendiscovery.org/rdf/Concept/HarmfulEmissions"/>
    <od:contains rdf:resource="http://opendiscovery.org/rdf/Concept/InternalHarmfulFactors"/>
    <od:contains rdf:resource="http://opendiscovery.org/rdf/Concept/LossOfEnergy"/>
    <od:contains rdf:resource="http://opendiscovery.org/rdf/Concept/LossOfInformation"/>
    <od:contains rdf:resource="http://opendiscovery.org/rdf/Concept/LossOfSubstance"/>
    <od:contains rdf:resource="http://opendiscovery.org/rdf/Concept/LossOfTime"/>
    <od:contains rdf:resource="http://opendiscovery.org/rdf/Concept/Noise"/>
    <skos:prefLabel xml:lang="en">Efficiency</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Effizienz</skos:prefLabel>
  </od:ParameterClass>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/EnergyUseOfTheMovingObject">
    <od:hasAltshuller71Id>19</od:hasAltshuller71Id>
    <od:hasAltshuller84Id>19</od:hasAltshuller84Id>
    <od:hasMatrix2003Id>16</od:hasMatrix2003Id>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">Consumption of energy resource(s) in order to perform the action or function of the object.</skos:definition>
    <skos:prefLabel xml:lang="de">Energieverbrauch des bewegten Objekts</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Energy consumption of the moving object</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Затрата энергии подвижного объекта</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/EnergyUseOfTheStationaryObject">
    <od:hasAltshuller71Id>20</od:hasAltshuller71Id>
    <od:hasAltshuller84Id>20</od:hasAltshuller84Id>
    <od:hasMatrix2003Id>17</od:hasMatrix2003Id>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">Consumption of energy resource(s) in order to perform the action or function of the object.</skos:definition>
    <skos:prefLabel xml:lang="de">Energieverbrauch des unbewegten Objekts</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Energy consumption of the stationary object</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Затрата энергии неподвижного объекта</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/ExternalHarmfulFactors">
    <od:hasAltshuller71Id>30</od:hasAltshuller71Id>
    <od:hasAltshuller84Id>30</od:hasAltshuller84Id>
    <od:hasMatrix2003Id>40</od:hasMatrix2003Id>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">Susceptibility of a system to externally generated (harmful) effects. A harmful effect is one that reduces the efficiency or quality of the functioning of the object or system.</skos:definition>
    <skos:definition xml:lang="de">Von außen auf das Objekt wirkende schädliche Faktoren</skos:definition>
    <skos:prefLabel xml:lang="en">External harmful factors</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Externe schädliche Faktoren</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Вредные факторы, действующие на объект извне</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/Force">
    <od:hasAltshuller71Id>10</od:hasAltshuller71Id>
    <od:hasAltshuller84Id>10</od:hasAltshuller84Id>
    <od:hasMatrix2003Id>15</od:hasMatrix2003Id>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">Force measures the interaction between systems and is measured as the strength or energy of physical action or movement. It is any interaction that changes an object’s condition or environment.</skos:definition>
    <skos:prefLabel xml:lang="en">Force (Intensity)</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Kraft</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Сила</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/FunctionEfficiency">
    <od:hasMatrix2003Id>24</od:hasMatrix2003Id>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Function Efficiency</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/HarmfulEmissions">
    <od:hasMatrix2003Id>30</od:hasMatrix2003Id>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Harmful Emissions</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/IlluminationIntensity">
    <od:hasAltshuller71Id>18</od:hasAltshuller71Id>
    <od:hasAltshuller84Id>18</od:hasAltshuller84Id>
    <od:hasMatrix2003Id>23</od:hasMatrix2003Id>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">Illumination characteristics of the system such as brightness, intensity, and color.</skos:definition>
    <skos:prefLabel xml:lang="de">Beleuchtungsintensität</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Illumination intensity</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Освещённость</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/InternalHarmfulFactors">
    <od:hasAltshuller71Id>31</od:hasAltshuller71Id>
    <od:hasAltshuller84Id>31</od:hasAltshuller84Id>
    <od:hasMatrix2003Id>31</od:hasMatrix2003Id>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">Harmful effects are generated by the object or system, as part of its operation.</skos:definition>
    <skos:definition xml:lang="de">Vom Objekt selbst erzeugte schädliche Faktoren</skos:definition>
    <skos:prefLabel xml:lang="en">Internal harmful factors</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Interne schädliche Faktoren</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Вредные факторы самого объекта</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/LengthOfTheMovingObject">
    <od:hasAltshuller71Id>03</od:hasAltshuller71Id>
    <od:hasAltshuller84Id>03</od:hasAltshuller84Id>
    <od:hasMatrix2003Id>03</od:hasMatrix2003Id>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">Any one linear dimension of the object, not necessarily the longest.</skos:definition>
    <skos:prefLabel xml:lang="en">Length of the moving object</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Länge des bewegten Objekts</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Длина подвижного объекта</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/LengthOfTheStationaryObject">
    <od:hasAltshuller71Id>04</od:hasAltshuller71Id>
    <od:hasAltshuller84Id>04</od:hasAltshuller84Id>
    <od:hasMatrix2003Id>04</od:hasMatrix2003Id>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">Any one linear dimension of the object, not necessarily the longest.</skos:definition>
    <skos:prefLabel xml:lang="en">Length of the stationary object</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Länge des unbewegten Objekts</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Длина неподвижного объекта</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/LevelOfAutomation">
    <od:hasAltshuller71Id>38</od:hasAltshuller71Id>
    <od:hasAltshuller84Id>38</od:hasAltshuller84Id>
    <od:hasMatrix2003Id>43</od:hasMatrix2003Id>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">The extent to which a system or object performs its functions without human assistance. At the highest level of operation, the system or object can determine what is needed, adapt to that need, and monitor its function.</skos:definition>
    <skos:prefLabel xml:lang="de">Automatisierungsgrad</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Level of automation</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Степень автоматизации</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/LossOfEnergy">
    <od:hasAltshuller71Id>22</od:hasAltshuller71Id>
    <od:hasAltshuller84Id>22</od:hasAltshuller84Id>
    <od:hasMatrix2003Id>27</od:hasMatrix2003Id>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">The waste of energy resource(s) through the function or movement of the object or system.</skos:definition>
    <skos:prefLabel xml:lang="de">Energieverluste</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Loss of Energy</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Потери энергии</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/LossOfInformation">
    <od:hasAltshuller71Id>24</od:hasAltshuller71Id>
    <od:hasAltshuller84Id>24</od:hasAltshuller84Id>
    <od:hasMatrix2003Id>28</od:hasMatrix2003Id>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">Partial or complete, permanent or temporary, loss of data or access to data in or by a system. Frequently includes sensory data such as aroma, texture, etc.</skos:definition>
    <skos:prefLabel xml:lang="de">Informationsverluste</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Loss of information</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Потери информации</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/LossOfSubstance">
    <od:hasAltshuller71Id>23</od:hasAltshuller71Id>
    <od:hasAltshuller84Id>23</od:hasAltshuller84Id>
    <od:hasMatrix2003Id>25</od:hasMatrix2003Id>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">Waste of a system’s materials, substances, parts, or subsystems during normal functioning.</skos:definition>
    <skos:prefLabel xml:lang="en">Loss of substance</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Materialverluste</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Потери вещества</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/LossOfTime">
