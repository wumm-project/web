<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF xmlns:cc="http://creativecommons.org/ns#"
   xmlns:dcterms="http://purl.org/dc/terms/"
   xmlns:od="http://opendiscovery.org/rdf/Model#"
   xmlns:owl="http://www.w3.org/2002/07/owl#"
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
   xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
   xmlns:skos="http://www.w3.org/2004/02/skos/core#"
   xmlns:tc="http://opendiscovery.org/rdf/Concept/"
   xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/Adaptability">
    <od:hasParameterNumber>35</od:hasParameterNumber>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Adaptionsfähigkeit, Universalität</skos:definition>
    <skos:definition xml:lang="en">The extent to which a system/object is appropriate for all situations or positively responds to external changes.</skos:definition>
    <skos:prefLabel xml:lang="en">Adaptability, Universality</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Adaptionsfähigkeit, Universalität</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Адаптивность, универсальность</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/BrightnessOfTheLighting">
    <od:hasParameterNumber>18</od:hasParameterNumber>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Beleuchtungsverhältnisse</skos:definition>
    <skos:definition xml:lang="en">Illumination characteristics of the system such as brightness, intensity, and color.</skos:definition>
    <skos:prefLabel xml:lang="de">Beleuchtungsverhältnisse</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Brightness of the lighting</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Видимость</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/ComplexityOfConstruction">
    <od:hasParameterNumber>36</od:hasParameterNumber>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Kompliziertheit der Struktur</skos:definition>
    <skos:definition xml:lang="en">The number and diversity of elements and element interrelationships comprising a system. Human interaction increases complexity.</skos:definition>
    <skos:prefLabel xml:lang="en">Complexity of construction</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Kompliziertheit der Struktur</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Сложность структуры</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/ComplexityOfInspectionAndMeasurements">
    <od:hasParameterNumber>37</od:hasParameterNumber>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">Demonstrated by measuring or monitoring systems that are complex, costly, require much time and labor to set up and use, or that have complex relationships between components or components that interfere with each other. Can also be indicated by the increasing cost of measuring to a satisfactory error.</skos:definition>
    <skos:definition xml:lang="de">Kompliziertheit der Kontrolle und Messung</skos:definition>
    <skos:prefLabel xml:lang="en">Complexity of inspection and measurements</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Kompliziertheit der Kontrolle und Messung</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Сложность контроля и измерений</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/EaseOfManufacture">
    <od:hasParameterNumber>32</od:hasParameterNumber>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Fertigungsfreundlichkeit</skos:definition>
    <skos:definition xml:lang="en">The degree of facility or effortlessness in manufacturing or fabricating the object/system.</skos:definition>
    <skos:prefLabel xml:lang="en">Ease of manufacture</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Fertigungsfreundlichkeit</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Удобство изготовления</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/EaseOfRepair">
    <od:hasParameterNumber>34</od:hasParameterNumber>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Instandsetzungsfreundlichkeit</skos:definition>
    <skos:definition xml:lang="en">The degree of facility or effortlessness in the mending or fixing of the system. Encompasses convenience as well as time to repair faults, failures, or defects.</skos:definition>
    <skos:prefLabel xml:lang="en">Ease of repair</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Instandsetzungsfreundlichkeit</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Удобство обслуживания</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/EaseOfUse">
    <od:hasParameterNumber>33</od:hasParameterNumber>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Bedienkomfort</skos:definition>
    <skos:definition xml:lang="en">Simple, easy to understand and perform or utilize; accessible.</skos:definition>
    <skos:prefLabel xml:lang="de">Bedienkomfort</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Ease of Use</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Простота использования</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/EnergyUseOfTheFixedObject">
    <od:hasParameterNumber>20</od:hasParameterNumber>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">Consumption of energy resource(s) in order to perform the action or function of the stationary object.</skos:definition>
    <skos:definition xml:lang="de">Energieverbrauch des unbeweglichen Objekts</skos:definition>
    <skos:prefLabel xml:lang="de">Energieverbrauch des unbeweglichen Objekts</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Energy use of the fixed object</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Энергопотребление недвижимого объекта</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/EnergyUseOfTheMovingObject">
    <od:hasParameterNumber>19</od:hasParameterNumber>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">Consumption of energy resource(s) in order to perform the action or function of the moving object.</skos:definition>
    <skos:definition xml:lang="de">Energieverbrauch des beweglichen Objekts</skos:definition>
    <skos:prefLabel xml:lang="de">Energieverbrauch des beweglichen Objekts</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Energy use of the moving object</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Энергопотребление движущегося объекта</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/ExternalDamagingFactors">
    <od:hasParameterNumber>30</od:hasParameterNumber>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">Susceptibility of a system to externally generated (harmful) effects. A harmful effect is one that reduces the efficiency or quality of the functioning of the object or system.</skos:definition>
    <skos:definition xml:lang="de">Von außen auf das Objekt wirkende schädliche Faktoren</skos:definition>
    <skos:prefLabel xml:lang="en">External damaging factors</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Externe schädliche Faktoren</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Вредные факторы, действующие на объект извне</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/Force">
    <od:hasParameterNumber>10</od:hasParameterNumber>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">Force measures the interaction between systems and is measured as the strength or energy of physical action or movement. It is any interaction that changes an object’s condition or environment.</skos:definition>
    <skos:definition xml:lang="de">Kraft</skos:definition>
    <skos:prefLabel xml:lang="en">Force</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Kraft</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Сила</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/FunctionalTimeOfTheMovingObject">
    <od:hasParameterNumber>15</od:hasParameterNumber>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Haltbarkeit des beweglichen Objekts</skos:definition>
    <skos:definition xml:lang="en">The time that the moving object or system can correctly perform its action. Also referred to as service life or durability.</skos:definition>
    <skos:prefLabel xml:lang="en">Functional time of the moving object</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Haltbarkeit des beweglichen Objekts</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Долговечность движущегося объекта</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/FunctionalTimeOfTheStationaryObject">
    <od:hasParameterNumber>16</od:hasParameterNumber>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Haltbarkeit des unbeweglichen Objekts</skos:definition>
    <skos:definition xml:lang="en">The time that the stationary object or system can correctly perform its action. Also referred to as service life or durability.</skos:definition>
    <skos:prefLabel xml:lang="en">Functional time of the stationary object</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Haltbarkeit des unbeweglichen Objekts</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Долговечность недвижимого объекта</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/InternalDamagingFactors">
    <od:hasParameterNumber>31</od:hasParameterNumber>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">Harmful effects are generated by the object or system, as part of its operation.</skos:definition>
    <skos:definition xml:lang="de">Vom Objekt selbst erzeugte schädliche Faktoren</skos:definition>
    <skos:prefLabel xml:lang="en">Internal damaging factors</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Interne schädliche Faktoren</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Вредные факторы, создаваемые самим объектом</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/LengthOfTheMovingObject">
    <od:hasParameterNumber>03</od:hasParameterNumber>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">Any one linear dimension of the moving object, not necessarily the longest.</skos:definition>
    <skos:definition xml:lang="de">Länge des beweglichen Objekts</skos:definition>
    <skos:prefLabel xml:lang="en">Length of the moving object</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Länge des beweglichen Objekts</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Длина движущегося объекта</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/LengthOfTheStationaryObject">
    <od:hasParameterNumber>04</od:hasParameterNumber>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">Any one linear dimension of the fixed or stationary object, not necessarily the longest.</skos:definition>
    <skos:definition xml:lang="de">Länge des unbeweglichen Objekts</skos:definition>
    <skos:prefLabel xml:lang="en">Length of the stationary object</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Länge des unbeweglichen Objekts</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Длина недвижимого объекта</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/LevelOfAutomation">
    <od:hasParameterNumber>38</od:hasParameterNumber>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Automatisierungsgrad</skos:definition>
    <skos:definition xml:lang="en">The extent to which a system or object performs its functions without human assistance. At the highest level of operation, the system or object can determine what is needed, adapt to that need, and monitor its function.</skos:definition>
    <skos:prefLabel xml:lang="de">Automatisierungsgrad</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Level of automation</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Уровень автоматизации</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/LossOfEnergy">
    <od:hasParameterNumber>22</od:hasParameterNumber>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Energieverluste</skos:definition>
    <skos:definition xml:lang="en">The waste of energy resource(s) through the function or movement of the object or system.</skos:definition>
    <skos:prefLabel xml:lang="de">Energieverluste</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Loss of Energy</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Потери энергии</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/LossOfInformation">
    <od:hasParameterNumber>24</od:hasParameterNumber>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Informationsverluste</skos:definition>
    <skos:definition xml:lang="en">Partial or complete, permanent or temporary, loss of data or access to data in or by a system. Frequently includes sensory data such as aroma, texture, etc.</skos:definition>
    <skos:prefLabel xml:lang="de">Informationsverluste</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Loss of information</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Потеря информации</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/LossOfMaterial">
    <od:hasParameterNumber>23</od:hasParameterNumber>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Materialverluste</skos:definition>
    <skos:definition xml:lang="en">Waste of a system’s materials, substances, parts, or subsystems during normal functioning.</skos:definition>
    <skos:prefLabel xml:lang="en">Loss of material</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Materialverluste</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Материальные потери</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/LossOfTime">
    <od:hasParameterNumber>25</od:hasParameterNumber>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">Time taken to perform the activity or function of the system or object. Improving the loss of time means reducing the time taken for the activity.</skos:definition>
    <skos:definition xml:lang="de">Zeitverlust</skos:definition>
    <skos:prefLabel xml:lang="en">Loss of time</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Zeitverlust</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Потери времени</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/Power">
    <od:hasParameterNumber>21</od:hasParameterNumber>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">Energy that is produced by mechanical, electrical, or other means. This characteristic refers to production not consumption.</skos:definition>
    <skos:definition xml:lang="de">Leistung, Kapazität</skos:definition>
    <skos:prefLabel xml:lang="de">Leistung, Kapazität</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Power</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Производительность, производительность</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/PrecisionOfManufacture">
    <od:hasParameterNumber>29</od:hasParameterNumber>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Fertigungsgenauigkeit</skos:definition>
    <skos:definition xml:lang="en">The extent to which the actual characteristics of the system or object match the specified or required characteristics.</skos:definition>
    <skos:prefLabel xml:lang="de">Fertigungsgenauigkeit</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Precision of manufacture</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Точность производства</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/PrecisionOfMeasurements">
    <od:hasParameterNumber>28</od:hasParameterNumber>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Messgenauigkeit</skos:definition>
    <skos:definition xml:lang="en">The degree to which the result of a measurement of a property of a system conforms to the correct value or a standard.</skos:definition>
    <skos:prefLabel xml:lang="de">Messgenauigkeit</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Precision of measurements</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Точность измерений</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/Productivity">
    <od:hasParameterNumber>39</od:hasParameterNumber>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Produktivität</skos:definition>
    <skos:definition xml:lang="en">The output of a system per unit time or per unit of input. The effectiveness of productive effort.</skos:definition>
    <skos:prefLabel xml:lang="en">Productivity</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Produktivität</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Производительность</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/QuantityOfMaterial">
    <od:hasParameterNumber>26</od:hasParameterNumber>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Materialmenge</skos:definition>
    <skos:definition xml:lang="en">The number or amount of a system’s materials, substances, parts, or subsystems which are altered (permanently or temporarily) or consumed (fully or partially) during normal functioning.</skos:definition>
    <skos:prefLabel xml:lang="de">Materialmenge</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Quantity of material</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Количество материала</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/Reliability">
    <od:hasParameterNumber>27</od:hasParameterNumber>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">A system’s ability to perform its intended functions in consistent, predictable ways.</skos:definition>
    <skos:definition xml:lang="de">Zuverlässigkeit</skos:definition>
    <skos:prefLabel xml:lang="en">Reliability</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Zuverlässigkeit</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Надежность</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/Shape">
    <od:hasParameterNumber>12</od:hasParameterNumber>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Form</skos:definition>
    <skos:definition xml:lang="en">The external contours, form, or appearance of a system.</skos:definition>
    <skos:prefLabel xml:lang="de">Form</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Shape</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Форма</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/Speed">
    <od:hasParameterNumber>09</od:hasParameterNumber>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Geschwindigkeit</skos:definition>
    <skos:definition xml:lang="en">The rate of a process or action, or the rate of movement of an object.</skos:definition>
    <skos:prefLabel xml:lang="de">Geschwindigkeit</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Speed</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Скорость</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/StabilityOfTheObjectsStructure">
    <od:hasParameterNumber>13</od:hasParameterNumber>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Stabilität der Struktur des Objekts</skos:definition>
    <skos:definition xml:lang="en">The standalone nature and immobility of the system or object.</skos:definition>
    <skos:prefLabel xml:lang="en">Stability of the object’s structure</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Stabilität der Struktur des Objekts</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Устойчивость композиции объекта</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/Strength">
    <od:hasParameterNumber>14</od:hasParameterNumber>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Festigkeit</skos:definition>
    <skos:definition xml:lang="en">The extent to which the object can resist changing in response to force. Resistance to breaking.</skos:definition>
    <skos:prefLabel xml:lang="de">Festigkeit</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Strength</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Сила</skos:prefLabel>
  </od:Parameter>
  <od:Parameter rdf:about="http://opendiscovery.org/rdf/Concept/SurfaceOfTheMovingObject">
    <od:hasParameterNumber>05</od:hasParameterNumber>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="de">Oberfläche des beweglichen Objekts</skos:definition>
