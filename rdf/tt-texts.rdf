    <tt:line>44</tt:line>
    <tt:sheet>148</tt:sheet>
    <tt:text xml:lang="de">Aktionsbereichsmodell</tt:text>
    <tt:text xml:lang="ru">Модель Действия в оперативной зоне</tt:text>
