    <tt:line>23</tt:line>
    <tt:sheet>104</tt:sheet>
    <tt:text xml:lang="de">1. Analyse einer Problemsituation</tt:text>
    <tt:text xml:lang="ru">1.Анализ проблемной ситуации</tt:text>
