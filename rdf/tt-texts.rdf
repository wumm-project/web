  <tt:Entry rdf:about="http://triztrainer.ru/rdf/Record/104_032">
    <tt:line>32</tt:line>
    <tt:sheet>104</tt:sheet>
    <tt:text xml:lang="de">Problemmodell</tt:text>
    <tt:text xml:lang="ru">Модель задачи</tt:text>
