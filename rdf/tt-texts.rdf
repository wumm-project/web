  <tt:Entry rdf:about="http://triztrainer.ru/rdf/Record/142_021">
    <tt:line>21</tt:line>
    <tt:sheet>142</tt:sheet>
    <tt:text xml:lang="de">Maschinenbedienung</tt:text>
    <tt:text xml:lang="ru">Работа машины</tt:text>
