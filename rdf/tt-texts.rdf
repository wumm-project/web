    <tt:line>81</tt:line>
    <tt:sheet>141</tt:sheet>
    <tt:text xml:lang="de">Um Haken, Gürtel und Kabel</tt:text>
    <tt:text xml:lang="ru">Вокруг крюка, пояса и троса</tt:text>
  </tt:Entry>
  <tt:Entry rdf:about="http://triztrainer.ru/rdf/Record/141_082">
    <tt:line>82</tt:line>
    <tt:sheet>141</tt:sheet>
    <tt:text xml:lang="de">Temporär</tt:text>
