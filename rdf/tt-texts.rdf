  <tt:Entry rdf:about="http://triztrainer.ru/rdf/Record/142_058">
    <tt:line>58</tt:line>
    <tt:sheet>142</tt:sheet>
    <tt:text xml:lang="de">1. Guter Chip 2. Defekter Chip 3. Lack 4. Kapazität 5. Leicht zugängliche Materialressourcen.</tt:text>
    <tt:text xml:lang="ru">1. Хорошая микросхема 2.Дефектная микросхема 3. Краска 4. Емкость 5. Легкодоступные вещественные ресурсы.</tt:text>
  </tt:Entry>
