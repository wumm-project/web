    <skos:prefLabel xml:lang="de">Verbesserung von Mess-Systemen</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Совершенствование измерительных систем</skos:prefLabel>
  </od:StandardSolution>
  <od:StandardSolution rdf:about="http://opendiscovery.org/rdf/Standard/S_431">
    <od:hasAltshullerLabel xml:lang="ru">Использование физэффектов</od:hasAltshullerLabel>
    <rdf:type rdf:resource="http://www.w3org/2004/02/skos/core#Concept"/>
    <skos:narrower rdf:resource="http://opendiscovery.org/rdf/Standard/S_43"/>
    <skos:prefLabel xml:lang="de">Nutze die Effekte-Datenbank zur Verbesserung von Mess-Systemen</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Use the Effects Database to Improve Measurement Systems</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Использовать базу данных эффектов для улучшения измерительных систем</skos:prefLabel>
  </od:StandardSolution>
  <od:StandardSolution rdf:about="http://opendiscovery.org/rdf/Standard/S_432">
    <od:hasAltshullerLabel xml:lang="ru">Использование резонанса контролируемого объекта</od:hasAltshullerLabel>
    <rdf:type rdf:resource="http://www.w3org/2004/02/skos/core#Concept"/>
    <skos:narrower rdf:resource="http://opendiscovery.org/rdf/Standard/S_43"/>
    <skos:prefLabel xml:lang="de">Nutze Resonanzphänomene zur Messung.</skos:prefLabel>
    <skos:prefLabel xml:lang="en">To determine the mass of a solid in a closed container, mechanical vibrations are excited, their frequency from the mass depends. </skos:prefLabel>
    <skos:prefLabel xml:lang="de">Um die Masse eines Festkörpers in einem geschlossen Behälter zu ermitteln, werden mechanische Schwingungen angeregt, deren Frequenz von der Masse abhängt.</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Use resonance phenomena to measure. </skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Для определения массы твердого тела в закрытом контейнере возбуждаются механические колебания, их частота зависит от массы зависит. </skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Используйте резонансные явления для измерения. </skos:prefLabel>
  </od:StandardSolution>
  <od:StandardSolution rdf:about="http://opendiscovery.org/rdf/Standard/S_433">
    <od:hasAltshullerLabel xml:lang="ru">Использование резонанса присоединенного объекта</od:hasAltshullerLabel>
    <rdf:type rdf:resource="http://www.w3org/2004/02/skos/core#Concept"/>
    <skos:narrower rdf:resource="http://opendiscovery.org/rdf/Standard/S_43"/>
    <skos:prefLabel xml:lang="de">Nutze Resonanzphänomene verknüpfter Objekte zur (indirekten) Messung</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Use resonant phenomena of linked objects for (indirect) measurement</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Использование резонансных явлений связанных объектов для (косвенного) измерения</skos:prefLabel>
  </od:StandardSolution>
  <od:StandardSolution rdf:about="http://opendiscovery.org/rdf/Standard/S_44">
    <od:hasAltshullerLabel xml:lang="ru">Переход к фепольным измерительным системам</od:hasAltshullerLabel>
    <rdf:type rdf:resource="http://www.w3org/2004/02/skos/core#Concept"/>
    <skos:narrower rdf:resource="http://opendiscovery.org/rdf/Standard/S_4"/>
