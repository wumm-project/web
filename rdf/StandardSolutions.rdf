  <od:StandardSolution rdf:about="http://opendiscovery.org/rdf/Standard/S_11">
    <od:hasAltshullerLabel xml:lang="ru">Синтез веполей</od:hasAltshullerLabel>
    <rdf:type rdf:resource="http://www.w3org/2004/02/skos/core#Concept"/>
    <skos:narrower rdf:resource="http://opendiscovery.org/rdf/Standard/S_1"/>
    <skos:prefLabel xml:lang="de">Aufbau von SF-Modellen</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Building su-field models</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Синтез веполей</skos:prefLabel>
  </od:StandardSolution>
  <od:StandardSolution rdf:about="http://opendiscovery.org/rdf/Standard/S_111">
    <od:hasAltshullerLabel xml:lang="ru">Синтез веполя</od:hasAltshullerLabel>
    <rdf:type rdf:resource="http://www.w3org/2004/02/skos/core#Concept"/>
    <skos:example xml:lang="en">To separate air or water (S1) from solid particles (S2), centrifuge (F, centrifugal force).</skos:example>
    <skos:example xml:lang="de">Um Luft oder Wasser (S1) von festen Partikeln (S2) abzutrennen, wird zentrifugiert (F, Zentrifugalkraft).</skos:example>
    <skos:example xml:lang="ru">Для отделения воздуха или воды (S1) от твердых частиц (S2), центрифуга (F, центробежная сила).</skos:example>
    <skos:narrower rdf:resource="http://opendiscovery.org/rdf/Standard/S_11"/>
    <skos:prefLabel xml:lang="en">Complete an incomplete su-field model</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Vervollständige ein unvollständiges Stoff-Feld-Modell</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Заполнить модель поля неполного вещества</skos:prefLabel>
  </od:StandardSolution>
  <od:StandardSolution rdf:about="http://opendiscovery.org/rdf/Standard/S_112">
    <od:hasAltshullerLabel xml:lang="ru">Переход к внутреннему комплексному веполю</od:hasAltshullerLabel>
    <rdf:type rdf:resource="http://www.w3org/2004/02/skos/core#Concept"/>
    <skos:example xml:lang="de">Kleine Flüssigkeitstropfen lassen sich leicht durch Zugabe eines Fluoreszenzfarbstoffes detektieren.</skos:example>
    <skos:example xml:lang="en">Small drops of liquid are easily detected by adding a fluorescent dye.</skos:example>
    <skos:example xml:lang="ru">Маленькие капли жидкости легко обнаруживаются при добавлении флуоресцентного красителя.</skos:example>
    <skos:narrower rdf:resource="http://opendiscovery.org/rdf/Standard/S_11"/>
    <skos:prefLabel xml:lang="en">If additives can be added internally, complete</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Wenn sich Additive intern zufügen lassen, vervollständige</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Если добавки можно добавлять изнутри, заполните</skos:prefLabel>
  </od:StandardSolution>
  <od:StandardSolution rdf:about="http://opendiscovery.org/rdf/Standard/S_113">
    <od:hasAltshullerLabel xml:lang="ru">Переход к внешнему комплексному веполю</od:hasAltshullerLabel>
    <rdf:type rdf:resource="http://www.w3org/2004/02/skos/core#Concept"/>
    <skos:example xml:lang="en">Gas leak detection by soap spray from the outside onto a pipe.</skos:example>
    <skos:example xml:lang="de">Gasleck-Detektion durch Seifenspray von außen auf ein Rohr.</skos:example>
    <skos:example xml:lang="ru">Обнаружение утечки газа с помощью мыльного спрея снаружи на трубу.</skos:example>
    <skos:narrower rdf:resource="http://opendiscovery.org/rdf/Standard/S_11"/>
    <skos:prefLabel xml:lang="en">If additives can be added externally, complete it with</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Wenn sich Additive extern zufügen lassen, vervollständige damit</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Если добавки можно добавлять извне, дополните их словом</skos:prefLabel>
  </od:StandardSolution>
  <od:StandardSolution rdf:about="http://opendiscovery.org/rdf/Standard/S_114">
    <od:hasAltshullerLabel xml:lang="ru">Переход к веполю на внешней среде</od:hasAltshullerLabel>
    <rdf:type rdf:resource="http://www.w3org/2004/02/skos/core#Concept"/>
    <skos:narrower rdf:resource="http://opendiscovery.org/rdf/Standard/S_11"/>
    <skos:prefLabel xml:lang="de">Nutze Ressourcen zur Vervollständigung</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Use resources to complete</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Использовать ресурсы для завершения</skos:prefLabel>
  </od:StandardSolution>
  <od:StandardSolution rdf:about="http://opendiscovery.org/rdf/Standard/S_115">
    <od:hasAltshullerLabel xml:lang="ru">Переход к веполю на внешней среде с добавками</od:hasAltshullerLabel>
    <rdf:type rdf:resource="http://www.w3org/2004/02/skos/core#Concept"/>
    <skos:narrower rdf:resource="http://opendiscovery.org/rdf/Standard/S_11"/>
    <skos:prefLabel xml:lang="en">Create more resources by changing the system environment</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Erzeuge weitere Ressourcen durch Veränderung der Systemumgebung</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Создавать больше ресурсов путем изменения системной среды</skos:prefLabel>
  </od:StandardSolution>
  <od:StandardSolution rdf:about="http://opendiscovery.org/rdf/Standard/S_116">
    <od:hasAltshullerLabel xml:lang="ru">Минимальный режим действия на вещество</od:hasAltshullerLabel>
    <rdf:type rdf:resource="http://www.w3org/2004/02/skos/core#Concept"/>
    <skos:narrower rdf:resource="http://opendiscovery.org/rdf/Standard/S_11"/>
    <skos:prefLabel xml:lang="de">Nutze überschüssige Aktionen zur Vervollständigung und eliminiere den Überschuß</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Use Excessive Action to Complete and Eliminate Excess</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Использовать чрезмерное действие для завершения и устранения избыточного</skos:prefLabel>
  </od:StandardSolution>
  <od:StandardSolution rdf:about="http://opendiscovery.org/rdf/Standard/S_117">
    <od:hasAltshullerLabel xml:lang="ru">Максимальный режим действия на вещество</od:hasAltshullerLabel>
    <rdf:type rdf:resource="http://www.w3org/2004/02/skos/core#Concept"/>
    <skos:narrower rdf:resource="http://opendiscovery.org/rdf/Standard/S_11"/>
    <skos:prefLabel xml:lang="en">If the excess action is harmful, try to redirect it to another component in the system</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Ist die überschüssige Aktion schädlich, dann versuche sie auf eine andere Komponente im System zu lenken</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Если избыточное действие вредно, попробуйте перенаправить его на другой компонент в системе</skos:prefLabel>
  </od:StandardSolution>
  <od:StandardSolution rdf:about="http://opendiscovery.org/rdf/Standard/S_118">
    <od:hasAltshullerLabel xml:lang="ru">Избирательно-максимальный режим</od:hasAltshullerLabel>
    <rdf:type rdf:resource="http://www.w3org/2004/02/skos/core#Concept"/>
    <skos:example xml:lang="de">Glasampullen werden beim Zuschmelzen in einem Wasserbad von unten her gekühlt, um Überhitzung des thermisch empfindlichen Inhalts zu vermeiden.</skos:example>
    <skos:example xml:lang="en">Glass ampoules are cooled from below when melted in a water bath to avoid overheating the thermally sensitive contents.</skos:example>
    <skos:example xml:lang="ru">Стеклянные ампулы охлаждают снизу при расплавлении на водяной бане, чтобы избежать перегрева термочувствительного содержимого.</skos:example>
    <skos:narrower rdf:resource="http://opendiscovery.org/rdf/Standard/S_11"/>
    <skos:prefLabel xml:lang="de">Führe zur Komplettierung lokal schützende Substanzen ein</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Implement local protective substances for completion</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Внедрить местные защитные вещества для завершения</skos:prefLabel>
  </od:StandardSolution>
  <od:StandardSolution rdf:about="http://opendiscovery.org/rdf/Standard/S_12">
    <od:hasAltshullerLabel xml:lang="ru">Разрушение веполей</od:hasAltshullerLabel>
    <rdf:type rdf:resource="http://www.w3org/2004/02/skos/core#Concept"/>
    <skos:narrower rdf:resource="http://opendiscovery.org/rdf/Standard/S_1"/>
    <skos:prefLabel xml:lang="en">Dissecting Fabric Field Models</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Zerlegung von Stoff-Feld-Modellen</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Рассеяние полевых моделей ткани</skos:prefLabel>
  </od:StandardSolution>
  <od:StandardSolution rdf:about="http://opendiscovery.org/rdf/Standard/S_121">
    <od:hasAltshullerLabel xml:lang="ru">Устранение вредной связи введением постороннего вещества</od:hasAltshullerLabel>
    <rdf:type rdf:resource="http://www.w3org/2004/02/skos/core#Concept"/>
    <skos:narrower rdf:resource="http://opendiscovery.org/rdf/Standard/S_12"/>
    <skos:prefLabel xml:lang="en">Eliminate Adverse Interactions by Introducing a Third Substance S3</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Eliminiere schädliche Interaktionen durch Einführung eines dritten Stoffes S3</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Устранить нежелательные взаимодействия путем введения третьего вещества S3</skos:prefLabel>
  </od:StandardSolution>
  <od:StandardSolution rdf:about="http://opendiscovery.org/rdf/Standard/S_122">
    <od:hasAltshullerLabel xml:lang="ru">Устранение вредной связи видоизменением имеющихся веществ</od:hasAltshullerLabel>
    <rdf:type rdf:resource="http://www.w3org/2004/02/skos/core#Concept"/>
    <skos:example xml:lang="de">Die oberflächliche Zerstörung von Rümpfen bei Schnellbooten durch Kavitation des Wassers kann durch Kühlung des Rumpfes und Ausbildung einer Eisschicht vermieden werden.</skos:example>
    <skos:example xml:lang="en">The superficial destruction of hulls in speedboats by cavitation of the water can be avoided by cooling the hull and forming a layer of ice.</skos:example>
    <skos:example xml:lang="ru">Поверхностного разрушения корпусов скоростных катеров путем кавитации воды можно избежать, охлаждая корпус и формируя слой льда.</skos:example>
    <skos:narrower rdf:resource="http://opendiscovery.org/rdf/Standard/S_12"/>
    <skos:prefLabel xml:lang="en">Eliminate harmful interactions by introducing a third substance S3, where S3 can be a modification of the two substances S1 and / or S2 present</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Eliminiere schädliche Interaktionen durch Einführung eines dritten Stoffes S3, wobei S3 eine Modifikation der beiden vorhandenen Stoffe S1 und/oder S2 sein kann</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Устранить вредные взаимодействия путем введения третьего вещества S3, где S3 может быть модификацией двух присутствующих веществ S1 и / или S2</skos:prefLabel>
  </od:StandardSolution>
  <od:StandardSolution rdf:about="http://opendiscovery.org/rdf/Standard/S_123">
    <od:hasAltshullerLabel xml:lang="ru">Оттягивание вредного действия поля</od:hasAltshullerLabel>
    <rdf:type rdf:resource="http://www.w3org/2004/02/skos/core#Concept"/>
    <skos:narrower rdf:resource="http://opendiscovery.org/rdf/Standard/S_12"/>
    <skos:prefLabel xml:lang="de">Lenke die schädliche Wirkung auf einen weniger wichtigen Stoff S3 z.B.: Wände von Behältern (S1), in denen Wasser (S2) gefriert, werden mit elastischen Materialien (S3) ausgekleidet, um die thermische Ausdehnung abzufangen und ein Beschädigen der Behälterwand auszuschließen.</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Reduce the harmful effect on a less important substance S3 eg: Walls of containers (S1) where water (S2) freezes lined with elastic materials (S3) to absorb the thermal expansion and to prevent damaging the container wall. </skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Уменьшить вредное воздействие на менее важное вещество S3, например: стены контейнеров (S1), где вода (S2) замерзает облицованы эластичными материалами (S3) для поглощения теплового расширения и предотвращения повреждения стенки контейнера. </skos:prefLabel>
  </od:StandardSolution>
  <od:StandardSolution rdf:about="http://opendiscovery.org/rdf/Standard/S_124">
    <od:hasAltshullerLabel xml:lang="ru">Противодействие вредным связям с помощью поля</od:hasAltshullerLabel>
    <rdf:type rdf:resource="http://www.w3org/2004/02/skos/core#Concept"/>
    <skos:example xml:lang="en">For pollination of flowers an air current is used, which unfortunately also causes the flowers to close, eliminating this negative effect by electrostatically charging the flowers which keeps them open. </skos:example>
    <skos:example xml:lang="de">Zur Bestäubung von Blumen wird eine Luftströmung eingesetzt, die leider auch zum Schließen der Blüten führt. Diesen negativen Effekt eliminiert man durch elektrostatische Aufladung der Blüten, wodurch sie offen bleiben.</skos:example>
    <skos:example xml:lang="ru">Для опыления цветов используется воздушный поток, который, к сожалению, также вызывает закрытие цветов, устраняя этот негативный эффект за счет электростатической зарядки цветов который держит их открытыми. </skos:example>
    <skos:narrower rdf:resource="http://opendiscovery.org/rdf/Standard/S_12"/>
    <skos:prefLabel xml:lang="de">Führe ein neues Feld zur Kompensation schädlicher Effekte ein</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Introduce a new field to compensate for harmful effects</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Введите новое поле для компенсации вредных воздействий</skos:prefLabel>
  </od:StandardSolution>
  <od:StandardSolution rdf:about="http://opendiscovery.org/rdf/Standard/S_125">
    <od:hasAltshullerLabel xml:lang="ru">"Отключение" магнитных связей</od:hasAltshullerLabel>
    <rdf:type rdf:resource="http://www.w3org/2004/02/skos/core#Concept"/>
    <skos:narrower rdf:resource="http://opendiscovery.org/rdf/Standard/S_12"/>
    <skos:prefLabel xml:lang="de">Nutze die Möglichkeit, Magnetfelder ein- und ausschalten zu können</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Take the opportunity to turn magnetic fields on and off</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Воспользуйтесь возможностью включать и выключать магнитные поля</skos:prefLabel>
  </od:StandardSolution>
  <od:StandardSolution rdf:about="http://opendiscovery.org/rdf/Standard/S_2">
    <od:hasAltshullerLabel xml:lang="ru">Развитие вепольных систем</od:hasAltshullerLabel>
    <rdf:type rdf:resource="http://www.w3org/2004/02/skos/core#Concept"/>
    <skos:narrower rdf:resource="http://opendiscovery.org/rdf/Standard/S_0"/>
    <skos:prefLabel xml:lang="en">Enhancing Fabric Field Models</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Verbesserung von Stoff-Feld-Modellen</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Улучшение полевых моделей тканей</skos:prefLabel>
  </od:StandardSolution>
  <od:StandardSolution rdf:about="http://opendiscovery.org/rdf/Standard/S_21">
    <od:hasAltshullerLabel xml:lang="ru">Переход к сложным веполям</od:hasAltshullerLabel>
    <rdf:type rdf:resource="http://www.w3org/2004/02/skos/core#Concept"/>
    <skos:narrower rdf:resource="http://opendiscovery.org/rdf/Standard/S_2"/>
    <skos:prefLabel xml:lang="en">Transitioning to More Complex Fabric Field Models</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Übergang zu komplexeren Stoff-Feld-Modellen</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Переход к более сложным моделям полевых структур</skos:prefLabel>
  </od:StandardSolution>
  <od:StandardSolution rdf:about="http://opendiscovery.org/rdf/Standard/S_211">
    <od:hasAltshullerLabel xml:lang="ru">Переход к цепному веполю</od:hasAltshullerLabel>
    <rdf:type rdf:resource="http://www.w3org/2004/02/skos/core#Concept"/>
    <skos:example xml:lang="en">Hilti principle, drive beats on heavy intermediate chisel, this on the actual chisel, this on the rock to be smashed (= impulse transmission and Reinforcement over several stages). </skos:example>
    <skos:example xml:lang="de">Hilti-Prinzip, Antrieb schlägt auf schweren Zwischenmeißel, dieser auf den eigentlichen Meißel, dieser auf den zu zerschlagenden Fels (= Impuls-Übertragung und -Verstärkung über mehrere Stufen).</skos:example>
    <skos:example xml:lang="ru">Принцип Hilti, приводные удары по тяжелому промежуточному долоту, это на фактическом долоте, это на скале, которая будет разбита (= импульсная передача и Армирование в несколько этапов). </skos:example>
    <skos:narrower rdf:resource="http://opendiscovery.org/rdf/Standard/S_21"/>
    <skos:prefLabel xml:lang="en">Link Multiple Fabric Field Models</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Verkette mehrere Stoff-Feld-Modelle</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Связать несколько полевых моделей матрицы</skos:prefLabel>
  </od:StandardSolution>
  <od:StandardSolution rdf:about="http://opendiscovery.org/rdf/Standard/S_212">
    <od:hasAltshullerLabel xml:lang="ru">Переход к двойному веполю</od:hasAltshullerLabel>
    <rdf:type rdf:resource="http://www.w3org/2004/02/skos/core#Concept"/>
    <skos:narrower rdf:resource="http://opendiscovery.org/rdf/Standard/S_21"/>
    <skos:prefLabel xml:lang="en">Double a Cloth-Field Model</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Verdopple ein Stoff-Feld-Modell</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Двойная модель поля ткани</skos:prefLabel>
  </od:StandardSolution>
  <od:StandardSolution rdf:about="http://opendiscovery.org/rdf/Standard/S_22">
    <od:hasAltshullerLabel xml:lang="ru">Форсирование веполей</od:hasAltshullerLabel>
    <rdf:type rdf:resource="http://www.w3org/2004/02/skos/core#Concept"/>
    <skos:narrower rdf:resource="http://opendiscovery.org/rdf/Standard/S_2"/>
    <skos:prefLabel xml:lang="en">Further Development of a Substance Field Model</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Weiterentwicklung eines Stoff-Feld-Modells</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Дальнейшая разработка модели поля вещества</skos:prefLabel>
  </od:StandardSolution>
  <od:StandardSolution rdf:about="http://opendiscovery.org/rdf/Standard/S_221">
    <od:hasAltshullerLabel xml:lang="ru">Переход к более управляемым полям</od:hasAltshullerLabel>
    <rdf:type rdf:resource="http://www.w3org/2004/02/skos/core#Concept"/>
    <skos:narrower rdf:resource="http://opendiscovery.org/rdf/Standard/S_22"/>
    <skos:prefLabel xml:lang="en">Put in better taxable fields</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Setze besser steuerbare Felder ein</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Поместите в более облагаемые налогом поля</skos:prefLabel>
  </od:StandardSolution>
  <od:StandardSolution rdf:about="http://opendiscovery.org/rdf/Standard/S_222">
    <od:hasAltshullerLabel xml:lang="ru">Дробление инструмента</od:hasAltshullerLabel>
    <rdf:type rdf:resource="http://www.w3org/2004/02/skos/core#Concept"/>
    <skos:example xml:lang="en">Knife - Knife with serration - Mirkowellenschliff - targeted roughened blade.</skos:example>
    <skos:example xml:lang="de">Messer - Messer mit Zahnung - Mirkowellenschliff - gezielt angerauhte Klinge.</skos:example>
