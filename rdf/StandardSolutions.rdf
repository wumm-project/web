  </od:StandardSolution>
  <od:StandardSolution rdf:about="http://opendiscovery.org/rdf/Standard/S_441">
    <od:hasAltshullerLabel xml:lang="ru">Переход к измерительному "протофеполю"</od:hasAltshullerLabel>
    <rdf:type rdf:resource="http://www.w3org/2004/02/skos/core#Concept"/>
    <skos:narrower rdf:resource="http://opendiscovery.org/rdf/Standard/S_44"/>
